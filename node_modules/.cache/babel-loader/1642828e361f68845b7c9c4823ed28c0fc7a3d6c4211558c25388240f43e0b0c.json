{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport CreateChannelModal from './CreateChannelModal.vue';\nexport default {\n  name: 'ChannelList',\n  components: {\n    CreateChannelModal\n  },\n  props: {\n    currentChannelId: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      expandedCategories: ['text', 'voice'],\n      // ÈªòËÆ§Â±ïÂºÄÁöÑÂàÜÁ±ª\n      showCreateChannel: false,\n      categories: [{\n        id: 'text',\n        name: 'ÊñáÂ≠óÈ¢ëÈÅì',\n        canCreateChannel: true,\n        channels: [{\n          id: 'general',\n          name: 'ÁªºÂêàËÆ®ËÆ∫',\n          type: 'text',\n          hasUnread: false\n        }, {\n          id: 'announcement',\n          name: 'ÂÖ¨Âëä',\n          type: 'text',\n          hasUnread: true,\n          unreadCount: 3\n        }]\n      }, {\n        id: 'voice',\n        name: 'ËØ≠Èü≥È¢ëÈÅì',\n        canCreateChannel: true,\n        channels: [{\n          id: 'voice-general',\n          name: 'ËØ≠Èü≥Â§ßÂéÖ',\n          type: 'voice',\n          hasUnread: false\n        }, {\n          id: 'gaming',\n          name: 'Ê∏∏ÊàèÈ¢ëÈÅì',\n          type: 'voice',\n          hasUnread: false\n        }]\n      }]\n    };\n  },\n  methods: {\n    // ÂàáÊç¢ÂàÜÁ±ªÂ±ïÂºÄ/ÊäòÂè†\n    toggleCategory(categoryId) {\n      const index = this.expandedCategories.indexOf(categoryId);\n      if (index === -1) {\n        this.expandedCategories.push(categoryId);\n      } else {\n        this.expandedCategories.splice(index, 1);\n      }\n    },\n    // ÈÄâÊã©È¢ëÈÅì\n    selectChannel(channel) {\n      this.$emit('select-channel', channel);\n    },\n    // ÊâìÂºÄÂàõÂª∫È¢ëÈÅìÊ®°ÊÄÅÊ°Ü\n    openCreateChannel() {\n      this.showCreateChannel = true;\n    },\n    // ÂÖ≥Èó≠ÂàõÂª∫È¢ëÈÅìÊ®°ÊÄÅÊ°Ü\n    closeCreateChannel() {\n      this.showCreateChannel = false;\n    },\n    // Â§ÑÁêÜ
avatar-upload\">\r\n      <div class=\"upload-section\">\r\n        <input\r\n          type=\"file\"\r\n          ref=\"fileInput\"\r\n          @change=\"handleFileSelect\"\r\n          accept=\"image/*\"\r\n          class=\"file-input\"\r\n          id=\"avatar-upload\"\r\n        />\r\n        <label for=\"avatar-upload\" class=\"upload-button\">\r\n          <i class=\"fas fa-upload\"></i>\r\n          上传头像\r\n        </label>\r\n      </div>\r\n\r\n      <!-- 头像编辑器弹窗 -->\r\n      <div v-if=\"showEditor\" class=\"editor-modal\">\r\n        <div class=\"editor-container\">\r\n          <UseCrop\r\n            ref=\"cropperRef\"\r\n            :src=\"imageUrl\"\r\n            :aspect=\"1\"\r\n            :minWidth=\"250\"\r\n            :minHeight=\"250\"\r\n            @change=\"onChange\"\r\n          />\r\n          <div class=\"editor-controls\">\r\n            <div class=\"editor-buttons\">\r\n              <button @click=\"saveAvatar\" class=\"save-button\">保存</button>\r\n              <button @click=\"cancelEdit\" class=\"cancel-button\">取消</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 预设头像选择 -->\r\n    <div class=\"preset-avatars\">\r\n      <h3>预设头像</h3>\r\n      <div class=\"avatar-grid\">\r\n        <div \r\n          v-for=\"(style, index) in avatarStyles\" \r\n          :key=\"style.id\"\r\n          class=\"preset-avatar\"\r\n          :class=\"{ selected: selectedPreset === index }\"\r\n          @click=\"selectPresetAvatar(style, index)\"\r\n        >\r\n          <div class=\"preset-avatar-wrapper\">\r\n            <img \r\n              :src=\"createAvatarUrl(style)\" \r\n              :alt=\"style.name\"\r\n              class=\"preset-avatar-image\"\r\n            />\r\n            <span class=\"preset-avatar-name\">{{ style.name }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { UseCrop } from '@vueuse/components'\r\nimport { createAvatar } from '@dicebear/core'\r\nimport * as adventurer from '@dicebear/adventurer'\r\nimport * as avataaars from '@dicebear/avataaars'\r\nimport * as bottts from '@dicebear/bottts'\r\nimport * as initials from '@dicebear/initials'\r\nimport * as shapes from '@dicebear/shapes'\r\nimport * as pixel from '@dicebear/pixel-art'\r\n\r\nexport default {\r\n  name: 'AvatarSelector',\r\n  components: {\r\n    UseCrop\r\n  },\r\n  props: {\r\n    username: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      selectedFile: null,\r\n      previewUrl: null,\r\n      showEditor: false,\r\n      imageUrl: '',\r\n      selectedPreset: -1,\r\n      avatarStyles: [\r\n        { id: 'initials', name: '简约', style: initials },\r\n        { id: 'bottts', name: '机器人', style: bottts },\r\n        { id: 'avataaars', name: '卡通', style: avataaars },\r\n        { id: 'shapes', name: '几何', style: shapes },\r\n        { id: 'adventurer', name: '冒险', style: adventurer },\r\n        { id: 'pixel', name: '像素', style: pixel }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    handleFileSelect(e) {\r\n      const file = e.target.files[0]\r\n      if (!file) return\r\n\r\n      if (!file.type.match('image.*')) {\r\n        alert('请选择图片文件')\r\n        return\r\n      }\r\n\r\n      if (file.size > 2 * 1024 * 1024) {\r\n        alert('图片大小不能超过2MB')\r\n        return\r\n      }\r\n\r\n      this.selectedFile = file\r\n      this.imageUrl = URL.createObjectURL(file)\r\n      this.showEditor = true\r\n      this.selectedPreset = -1\r\n    },\r\n\r\n    onChange(data) {\r\n      if (data && data.imageData) {\r\n        this.previewUrl = data.imageData\r\n      }\r\n    },\r\n\r\n    async saveAvatar() {\r\n      if (this.$refs.cropperRef && this.previewUrl) {\r\n        this.showEditor = false\r\n        this.$emit('update:avatar', this.previewUrl)\r\n      }\r\n    },\r\n\r\n    cancelEdit() {\r\n      this.showEditor = false\r\n      this.selectedFile = null\r\n      this.imageUrl = ''\r\n      if (this.$refs.fileInput) {\r\n        this.$refs.fileInput.value = ''\r\n      }\r\n    },\r\n\r\n    createAvatarUrl(styleConfig) {\r\n      try {\r\n        const options = {\r\n          seed: this.username,\r\n          size: 128,\r\n          radius: 50,\r\n          ...this.getStyleSpecificOptions(styleConfig.id)\r\n        }\r\n        \r\n        const avatar = createAvatar(styleConfig.style, options)\r\n        return avatar.toDataUriSync()\r\n      } catch (error) {\r\n        console.error('创建头像失败:', error)\r\n        return null\r\n      }\r\n    },\r\n\r\n    getStyleSpecificOptions(styleId) {\r\n      const options = {\r\n        initials: {\r\n          backgroundColor: ['b6e3f4', 'c0aede', 'aff6cf'],\r\n          fontSize: 50\r\n        },\r\n        bottts: {\r\n          colorful: true,\r\n          primaryColorLevel: 600,\r\n          secondaryColorLevel: 400\r\n        },\r\n        avataaars: {\r\n          backgroundColor: ['b6e3f4', 'c0aede', 'aff6cf'],\r\n          clothingColor: ['red', 'blue', 'green'],\r\n          accessoriesProbability: 50\r\n        },\r\n        shapes: {\r\n          backgroundColor: ['b6e3f4', 'c0aede', 'aff6cf'],\r\n          shape1: ['circle', 'square', 'triangle'],\r\n          shape2: ['circle', 'square', 'triangle'],\r\n          shape3: ['circle', 'square', 'triangle']\r\n        },\r\n        adventurer: {\r\n          backgroundColor: ['b6e3f4', 'c0aede', 'aff6cf']\r\n        },\r\n        pixel: {\r\n          backgroundColor: ['b6e3f4', 'c0aede', 'aff6cf'],\r\n          colorful: true\r\n        }\r\n      }\r\n      return options[styleId] || {}\r\n    },\r\n\r\n    selectPresetAvatar(style, index) {\r\n      this.selectedPreset = index\r\n      this.selectedFile = null\r\n      this.showEditor = false\r\n      \r\n      const avatarUrl = this.createAvatarUrl(style)\r\n      if (avatarUrl) {\r\n        this.previewUrl = avatarUrl\r\n        this.$emit('update:avatar', avatarUrl)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.avatar-selector {\r\n  width: 100%;\r\n  max-width: 500px;\r\n  padding: 1.5rem;\r\n  background-color: var(--input-background);\r\n  border-radius: var(--card-border-radius);\r\n  box-shadow: var(--message-shadow);\r\n}\r\n\r\n.avatar-preview {\r\n  width: 120px;\r\n  height: 120px;\r\n  margin: 0 auto 1.5rem;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  border: 2px dashed var(--primary-color);\r\n  background-color: transparent;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  cursor: pointer;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.avatar-preview:hover {\r\n  border-color: var(--secondary-color);\r\n  transform: scale(1.02);\r\n}\r\n\r\n.preview-image {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n  transition: transform 0.2s ease;\r\n}\r\n\r\n.preview-image:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.empty-preview {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 100%;\r\n  height: 100%;\r\n  cursor: pointer;\r\n  background-color: var(--input-background);\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.empty-preview:hover {\r\n  background-color: var(--other-message-bg);\r\n}\r\n\r\n.empty-preview i {\r\n  font-size: 2rem;\r\n  color: var(--primary-color);\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.empty-preview-text {\r\n  font-size: 0.9rem;\r\n  color: var(--text-color);\r\n  opacity: 0.8;\r\n}\r\n\r\n.avatar-upload {\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.upload-section {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.file-input {\r\n  display: none;\r\n}\r\n\r\n.upload-button {\r\n  padding: 0.75rem 1.5rem;\r\n  background-color: var(--primary-color);\r\n  color: white;\r\n  border-radius: var(--card-border-radius);\r\n  cursor: pointer;\r\n  transition: all 0.2s ease;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.upload-button:hover {\r\n  background-color: var(--secondary-color);\r\n  transform: translateY(-2px);\r\n}\r\n\r\n.editor-modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgba(0, 0, 0, 0.8);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1000;\r\n}\r\n\r\n.editor-container {\r\n  background-color: var(--chat-background);\r\n  padding: 2rem;\r\n  border-radius: var(--card-border-radius);\r\n  box-shadow: var(--message-shadow);\r\n  max-width: 90vw;\r\n  max-height: 90vh;\r\n  overflow: auto;\r\n}\r\n\r\n.editor-controls {\r\n  margin-top: 1.5rem;\r\n}\r\n\r\n.editor-buttons {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n\r\n.save-button,\r\n.cancel-button {\r\n  padding: 0.5rem 1rem;\r\n  border: none;\r\n  border-radius: var(--card-border-radius);\r\n  cursor: pointer;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.save-button {\r\n  background-color: var(--primary-color);\r\n  color: white;\r\n}\r\n\r\n.save-button:hover {\r\n  background-color: var(--secondary-color);\r\n}\r\n\r\n.cancel-button {\r\n  background-color: var(--other-message-bg);\r\n  color: var(--text-color);\r\n}\r\n\r\n.cancel-button:hover {\r\n  background-color: var(--input-background);\r\n}\r\n\r\n.preset-avatars {\r\n  margin-top: 2rem;\r\n}\r\n\r\n.preset-avatars h3 {\r\n  color: var(--text-color);\r\n  margin-bottom: 1rem;\r\n  font-size: 1.1rem;\r\n}\r\n\r\n.avatar-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));\r\n  gap: 1rem;\r\n  padding: 1rem;\r\n}\r\n\r\n.preset-avatar-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.preset-avatar-name {\r\n  font-size: 0.8rem;\r\n  color: var(--text-color);\r\n  opacity: 0.8;\r\n  text-align: center;\r\n}\r\n\r\n.preset-avatar {\r\n  width: 80px;\r\n  height: 80px;\r\n  paddi
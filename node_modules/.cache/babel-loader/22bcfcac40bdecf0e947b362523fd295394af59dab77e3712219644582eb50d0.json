100vh;\n  padding: 1rem;\n}\n\n/* Discord布局 */\n.discord-layout {\n  display: flex;\n  width: 100%;\n  height: 100vh;\n  overflow: hidden;\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n  .chat-main {\n    flex-direction: column;\n  }\n  \n  .discord-layout {\n    flex-direction: column;\n  }\n  \n  .server-list {\n    width: 100% !important;\n    height: auto !important;\n    flex-direction: row !important;\n    padding: 8px !important;\n  }\n  \n  .server-items {\n    flex-direction: row !important;\n    padding: 0 8px !important;\n  }\n  \n  .server-item {\n    margin-right: 8px !important;\n    margin-bottom: 0 !important;\n  }\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAe;;EAD5BC,GAAA;EAE0BD,KAAK,EAAC;;;EAFhCC,GAAA;EAKgBD,KAAK,EAAC;;;;;;;;uBAJpBE,mBAAA,CA8BM,OA9BNC,UA8BM,G,CA7BQC,IAAA,CAAAC,QAAQ,I,cAApBH,mBAAA,CAEM,OAFNI,UAEM,GADJC,YAAA,CAAkCC,oBAAA;IAAtBC,OAAK,EAAEL,IAAA,CAAAM;EAAW,qC,oBAEhCR,mBAAA,CAyBM,OAzBNS,UAyBM,GAxBJC,mBAAA,WAAc,EACdL,YAAA,CAAcM,qBAAA,GAEdD,mBAAA,YAAe,EACfL,YAAA,CAIEO,6BAAA;IAHCC,WAAW,EAAEX,IAAA,CAAAW,WAAW;IACxBC,eAAe,EAAEZ,IAAA,CAAAC,QAAQ;IACzBY,mBAAmB,EAAEb,IAAA,CAAAc;sFAGxBN,mBAAA,UAAa,EACbL,YAAA,CAIEY,wBAAA;IAHCC,WAAW;MAAAf,QAAA,EAAID,IAAA,CAAAC,QAAQ;MAAAgB,MAAA,EAAEjB,IAAA,CAAAiB;IAAM;IAC/BC,mBAAmB,EAAElB,IAAA,CAAAmB,iBAAiB;IACtCN,mBAAmB,EAAEb,IAAA,CAAAc;0FAGxBN,mBAAA,aAAgB,EAERR,IAAA,CAAAoB,iBAAiB,I,cADzBC,YAAA,CAKEC,4BAAA;IA7BRzB,GAAA;IA0BS0B,MAAM,EAAEvB,IAAA,CAAAoB,iBAAiB;IACzBI,OAAK,EAAExB,IAAA,CAAAyB,kBAAkB;IACzBC,eAAc,EAAE1B,IAAA,CAAA2B;uEA5BzBnB,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
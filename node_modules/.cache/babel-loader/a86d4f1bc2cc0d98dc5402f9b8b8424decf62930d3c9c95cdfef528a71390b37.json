{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { Cropper } from 'vue-advanced-cropper';\nimport 'vue-advanced-cropper/dist/style.css';\nimport { createAvatar } from '@dicebear/core';\nimport * as initials from '@dicebear/initials';\nimport * as bottts from '@dicebear/bottts';\nimport * as miniavs from '@dicebear/miniavs';\nimport * as pixelArt from '@dicebear/pixel-art';\nimport VueAvatarEditor from 'vue-avatar-editor';\nexport default {\n  name: 'AvatarSelector',\n  components: {\n    Cropper,\n    VueAvatarEditor\n  },\n  props: {\n    username: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      selectedFile: null,\n      previewUrl: null,\n      showEditor: false,\n      zoom: 1,\n      selectedPreset: null,\n      avatarStyles: [{\n        id: 'initials',\n        name: '字母',\n        options: {\n          backgroundColor: ['FF6B6B', '4ECDC4', '45B7D1', '96CEB4'],\n          fontSize: 50,\n          bold: true\n        }\n      }, {\n        id: 'bottts',\n        name: '机器人',\n        options: {\n          primaryColorLevel: 600,\n          secondaryColorLevel: 400,\n          mouthChance: 100,\n          sidesChance: 100,\n          textureChance: 50\n        }\n      }, {\n        id: 'miniavs',\n        name: '卡通',\n        options: {\n          mouth: ['smile', 'laughing', 'peaceful'],\n          eyes: ['normal', 'happy', 'wink'],\n          backgroundColor: ['FFD93D', 'FF6B6B', '4ECDC4']\n        }\n      }, {\n        id: 'pixel',\n        name: '像素',\n        options: {\n          mood: ['happy'],\n          backgroundColor: ['45B7D1', '96CEB4', 'FFD93D'],\n          colorful: true,\n          pixelSize: 8\n        }\n      }]\n    };\n  },\n  mounted() {\n    this.validateAvatarStyles();\n  },\n  methods: {\n    validateAvatarStyles() {\n      this.avatarStyles = this.avatarStyles.filter(style => {\n        try {\n          const avatar = createAvatar(style.style, {\n            seed: 'test',\n            ...style.options\n          });\n          avatar.toDataUriSync();\n          return true;\n        } catch (error) {\n          console.error(`头像风格 ${style.name} 加载失败:`, error);\n          return false;\n        }\n      });\n    },\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    handleFileSelect(event) {\n      const file = event.target.files[0];\n      if (file && file.type.startsWith('image/')) {\n        this.selectedFile = file;\n        this.selectedPreset = null;\n        this.showEditor = true;\n        this.zoom = 1;\n      }\n    },\n    handleImageReady(image) {\n      this.previewUrl = image;\n    },\n    async saveAvatar() {\n      try {\n        if (this.$refs.avatarEditor) {\n          const canvas = this.$refs.avatarEditor.getImageScaledToCanvas();\n          const dataUrl = canvas.toDataURL('image/png');\n          this.previewUrl = dataUrl;\n          this.showEditor = false;\n          this.$emit('avatar-selected', dataUrl);\n        }\n      } catch (error) {\n        console.error('保存头像时出错:', error);\n      }\n    },\n    cancelEdit() {\n      this.showEditor = false;\n      this.selectedFile = null;\n      if (!this.selectedPreset) {\n        this.previewUrl = null;\n      }\n    },\n    selectPresetAvatar(style) {\n      this.selectedPreset = style.id;\n      this.selectedFile = null;\n      this.showEditor = false;\n      const avatarUrl = this.createAvatarUrl(style);\n      this.previewUrl = avatarUrl;\n      this.$emit('avatar-selected', avatarUrl);\n    },\n    createAvatarUrl(style) {\n      try {\n        const seed = this.username || Math.random().toString(36).substring(7);\n        const options = {\n          ...style.options,\n          size: 128\n        };\n        switch (style.id) {\n          case 'initials':\n            return `https://api.dicebear.com/7.x/initials/svg?seed=${seed}&${this.serializeOptions(options)}`;\n          case 'bottts':\n            return `https://api.dicebear.com/7.x/bottts/svg?seed=${seed}&${this.serializeOptions(options)}`;\n          case 'miniavs':\n            return `https://api.dicebear.com/7.x/miniavs/svg?seed=${seed}&${this.serializeOptions(options)}`;\n          case 'pixel':\n            return `https://api.dicebear.com/7.x/pixel-art/svg?seed=${seed}&${this.serializeOptions(options)}`;\n          default:\n            return '';\n        }\n      } catch (error) {\n        console.error(`创建头像URL时出错 (${style.id}):`, error);\n        return '';\n      }\n    },\n    serializeOptions(options) {\n      return Object.entries(options).map(([key, value]) => {\n        if (Array.isArray(value)) {\n          return `${key}=${value.join(',')}`;\n        }\n        return `${key}=${value}`;\n      }).join('&');\n    },\n    handleAvatarError(styleId) {\n      console.error(`头像风格 ${styleId} 加载失败`);\n    }\n  }\n};","map":{"version":3,"names":["Cropper","createAvatar","initials","bottts","miniavs","pixelArt","VueAvatarEditor","name","components","props","username","type","String","default","data","selectedFile","previewUrl","showEditor","zoom","selectedPreset","avatarStyles","id","options","backgroundColor","fontSize","bold","primaryColorLevel","secondaryColorLevel","mouthChance","sidesChance","textureChance","mouth","eyes","mood","colorful","pixelSize","mounted","validateAvatarStyles","methods","filter","style","avatar","seed","toDataUriSync","error","console","triggerFileInput","$refs","fileInput","click","handleFileSelect","event","file","target","files","startsWith","handleImageReady","image","saveAvatar","avatarEditor","canvas","getImageScaledToCanvas","dataUrl","toDataURL","$emit","cancelEdit","selectPresetAvatar","avatarUrl","createAvatarUrl","Math","random","toString","substring","size","serializeOptions","Object","entries","map","key","value","Array","isArray","join","handleAvatarError","styleId"],"sources":["C:\\Users\\Administrator\\Desktop\\discord-chat-app\\fack\\just-do-it\\src\\components\\chat\\AvatarSelector.vue"],"sourcesContent":["<template>\n  <div class=\"avatar-selector\">\n    <!-- 头像预览区域 -->\n    <div class=\"avatar-preview\" @click=\"triggerFileInput\">\n      <div v-if=\"previewUrl\" class=\"avatar-preview-container\">\n        <img :src=\"previewUrl\" alt=\"Avatar preview\" class=\"preview-image\" />\n        <div class=\"avatar-hover-effect\"></div>\n      </div>\n      <div v-else class=\"empty-preview\">\n        <div class=\"upload-icon\"></div>\n        <span>更换头像</span>\n      </div>\n      <input\n        type=\"file\"\n        ref=\"fileInput\"\n        @change=\"handleFileSelect\"\n        accept=\"image/*\"\n        style=\"display: none\"\n      />\n    </div>\n\n    <!-- 头像编辑器模态框 -->\n    <div v-if=\"showEditor\" class=\"editor-modal\">\n      <div class=\"editor-container\">\n        <vue-avatar-editor\n          ref=\"avatarEditor\"\n          :image=\"selectedFile\"\n          :width=\"250\"\n          :height=\"250\"\n          :border=\"50\"\n          :color=\"[255, 255, 255, 0.6]\"\n          :scale=\"zoom\"\n          @vue-avatar-editor:image-ready=\"handleImageReady\"\n        />\n        <div class=\"editor-controls\">\n          <input\n            type=\"range\"\n            min=\"1\"\n            max=\"2\"\n            step=\"0.01\"\n            v-model=\"zoom\"\n            class=\"zoom-slider\"\n          />\n          <div class=\"editor-buttons\">\n            <button @click=\"saveAvatar\" class=\"save-button\">保存</button>\n            <button @click=\"cancelEdit\" class=\"cancel-button\">取消</button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 预设头像选择区域 -->\n    <div class=\"preset-avatars\">\n      <h3>选择头像风格</h3>\n      <div class=\"avatar-grid\">\n        <div\n          v-for=\"style in avatarStyles\"\n          :key=\"style.id\"\n          class=\"preset-avatar-wrapper\"\n        >\n      
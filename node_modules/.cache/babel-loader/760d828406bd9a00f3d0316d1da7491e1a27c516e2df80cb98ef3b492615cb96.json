{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'CreateServerModal',\n  props: {\n    isOpen: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      currentStep: 1,\n      selectedType: '',\n      selectedInterests: [],\n      serverName: '',\n      serverIcon: null,\n      // ÊúçÂä°Âô®Á±ªÂûãÈÄâÈ°π\n      serverTypes: [{\n        id: 'gaming',\n        name: 'Ê∏∏ÊàèÁ§æÂå∫',\n        description: 'ÂàõÂª∫‰∏Ä‰∏™Ê∏∏ÊàèÁé©ÂÆ∂ÁöÑ‰∫§ÊµÅÁ§æÂå∫',\n        icon: 'üéÆ'\n      }, {\n        id: 'study',\n        name: 'Â≠¶‰π†Â∞èÁªÑ',\n        description: 'ÂàõÂª∫‰∏Ä‰∏™Â≠¶‰π†‰∫§ÊµÅÁöÑÁ©∫Èó¥',\n        icon: 'üìö'\n      }, {\n        id: 'social',\n        name: 'Á§æ‰∫§ËÅäÂ§©',\n        description: 'ÂàõÂª∫‰∏Ä‰∏™ÊúãÂèãÈó¥ÁöÑËÅäÂ§©Á©∫Èó¥',\n        icon: 'üí¨'\n      }, {\n        id: 'other',\n        name: 'ÂÖ∂‰ªñ',\n        description: 'ÂàõÂª∫‰∏Ä‰∏™Ëá™ÂÆö‰πâÁ±ªÂûãÁöÑÊúçÂä°Âô®',\n        icon: 'üåü'\n      }],\n      // ÂÖ¥Ë∂£Ê†áÁ≠æÈÄâÈ°π\n      interests: [{\n        id: 'games',\n        name: 'Ê∏∏Êàè',\n        icon: 'üéÆ'\n      }, {\n        id: 'music',\n        name: 'Èü≥‰πê',\n        icon: 'üéµ'\n      }, {\n        id: 'art',\n        name: 'Ëâ∫ÊúØ',\n        icon: 'üé®'\n      }, {\n        id: 'tech',\n        name: 'ÁßëÊäÄ',\n        icon: 'üíª'\n      }, {\n        id: 'sports',\n        name: 'ËøêÂä®',\n        icon: '‚öΩ'\n      }, {\n        id: 'food',\n        name: 'ÁæéÈ£ü',\n        icon: 'üçú'\n      }, {\n        id: 'travel',\n        name: 'ÊóÖË°å',\n        icon: '‚úàÔ∏è'\n      }, {\n        id: 'movies',\n        name: 'ÁîµÂΩ±',\n        icon: 'üé¨'\n      }, {\n        id: 'books',\n        name: 'ËØª‰π¶',\n        icon: 'üìö'\n      }, {\n        id: 'pets',\n        name: 'ÂÆ†Áâ©',\n        icon: 'üê±'\n      }, {\n        id: 'fashion',\n        name: 'Êó∂Â∞ö',\n        icon: 'üëó'\n      }, {\n        id: 'education',\n        name: 'ÊïôËÇ≤',\n        icon: 'üìñ'\n      }]\n    };\n  },\n  computed: {\n    // Âà§Êñ≠ÊòØÂê¶ÂèØ‰ª•ËøõË°å‰∏ã‰∏ÄÊ≠•\n    canProceed() {\n      switch (this.currentStep) {\n        case 1:\n          return !!this.selectedType;\n        case 2:\n          return this.selectedInterests.length > 0;\n        default:\n          return true;\n      }\n    },\n    // Âà§Êñ≠ÊòØÂê¶ÂèØ‰ª•ÂàõÂª∫ÊúçÂä°Âô®\n    canCreate() {\n      return this.serverName.trim().length > 0;\n    }\n  },\n  methods: {\n    // ÂÖ≥Èó≠Ê®°ÊÄÅÊ°Ü\n    closeModal() {\n      this.$emit('close');\n      this.resetForm();\n    },\n    // ÂàáÊç¢ÂÖ¥Ë∂£Ê†áÁ≠æÁöÑÈÄâ‰∏≠Áä∂ÊÄÅ\n    toggleInterest(interestId) {\n      const index = this.selectedInterests.indexOf(interestId);\n      if (index === -1) {\n        if (this.selectedInterests.length < 5) {\n          // ÊúÄÂ§öÈÄâÊã©5‰∏™ÂÖ¥Ë∂£\n          this.selectedInterests.push(interestId);\n        }\n      } else {\n        this.selectedInterests.splice(index, 1);\n      }\n    },\n    // Ëß¶ÂèëÊñá‰ª∂ÈÄâÊã©\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    // Â§ÑÁêÜÂõæÊ†á‰∏ä‰º†\n    async handleIconUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        try {\n          const reader = new FileReader();\n          reader.onload = e => {\n            this.serverIcon = e.target.result;\n          };\n          reader.readAsDataURL(file);\n        } catch (error) {\n          console.error('ÂõæÊ†á‰∏ä‰º†Â§±Ë¥•:', error);\n        }\n      }\n    },\n    // ÂàõÂª∫ÊúçÂä°Âô®\n    createServer() {\n      const serverData = {\n        name: this.serverName,\n        type: this.selectedType,\n        interests: this.selectedInterests,\n        icon: this.serverIcon\n      };\n      this.$emit('create', serverData);\n      this.closeModal();\n    },\n    // ÈáçÁΩÆË°®Âçï\n    resetForm() {\n      this.currentStep = 1;\n      this.selectedType = '';\n      this.selectedInterests = [];\n      this.serverName = '';\n      this.serverIcon = null;\n    }\n  }\n};","map":{"version":3,"names":["name","props","isOpen","type","Boolean","default","data","currentStep","selectedType","selectedInterests","serverName","serverIcon","serverTypes","id","description","icon","interests","computed","canProceed","length","canCreate","trim","methods","closeModal","$emit","resetForm","toggleInterest","interestId","index","indexOf","push","splice","triggerFileInput","$refs","fileInput","click","handleIconUpload","event","file","target","files","reader","FileReader","onload","e","result","readAsDataURL","error","console","createServer","serverData"],"sources":["C:\\Users\\Administrator\\Desktop\\who\\src\\components\\chat\\CreateServerModal.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isOpen\" class=\"create-server-modal\">\r\n    <div class=\"modal-overlay\" @click=\"closeModal\"></div>\r\n    <div class=\"modal-content\">\r\n      <!-- Ê≠•È™§ÊåáÁ§∫Âô® -->\r\n      <div class=\"step-indicator\">\r\n        <div class=\"step-dots\">\r\n          <div \r\n            v-for=\"step in 3\" \r\n            :key=\"step\"\r\n            class=\"step-dot\"\r\n            :class=\"{ active: currentStep === step }\"\r\n          ></div>\r\n        </div>\r\n        <span class=\"step-text\">Á¨¨ {{ currentStep }}/3 Ê≠•</span>\r\n      </div>\r\n\r\n      <!-- Á¨¨‰∏ÄÊ≠•ÔºöÈÄâÊã©ÊúçÂä°Âô®Á±ªÂûã -->\r\n      <div v-if=\"currentStep === 1\" class=\"step-content\">\r\n        <h2>ÂàõÂª∫ÊúçÂä°Âô®</h2>\r\n        <p class=\"step-description\">ÈÄâÊã©‰∏Ä‰∏™Á±ªÂûãÔºåËÆ©Êàë‰ª¨Â∏Æ‰Ω†ËÆæÁΩÆÊúçÂä°Âô®</p>\r\n        \r\n        <div class=\"server-types\">\r\n          <div \r\n            v-for=\"type in serverTypes\" \r\n            :key=\"type.id\"\r\n            class=\"type-option\"\r\n            :class=\"{ selected: selectedType === type.id }\"\r\n            @click=\"selectedType = type.id\"\r\n          >\r\n            <div class=\"type-icon\">{{ type.icon }}</div>\r\n            <div class=\"type-info\">\r\n              <h3>{{ type.name }}</h3>\r\n              <p>{{ type.description }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Á¨¨‰∫åÊ≠•ÔºöÈÄâÊã©ÂÖ¥Ë∂£Ê†áÁ≠æ -->\r\n      <div v-if=\"currentStep === 2\" class=\"step-content\">\r\n        <h2>ÈÄâÊã©ÊúçÂä°Âô®ÂÖ¥Ë∂£</h2>\r\n        <p class=\"step-description\">ÈÄâÊã©‰∏Ä‰∫õÂÖ¥Ë∂£Ê†áÁ≠æÔºåÂ∏ÆÂä©ÂÖ∂‰ªñ‰∫∫‰∫ÜËß£‰Ω†ÁöÑÊúçÂä°Âô®</p>\r\n        \r\n        <div class=\"interests-grid\">\r\n          <div \r\n            v-for=\"interest in interests\" \r\n            :key=\"interest.id\"\r\n            class=\"interest-tag\"\r\n            :class=\"{ selected: selectedInterests.includes(interest.id) }\"\r\n            @click=\"toggleInterest(interest.id)\"\r\n          >\r\n            <span class=\"interest-icon\">{{ interest.icon }}</span>\r\n            {{ interest.name }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Á¨¨‰∏âÊ≠•ÔºöËÆæÁΩÆÊúçÂä°Âô®ËØ¶ÊÉÖ -->\r\n      <div v-if=\"currentStep === 3\" class=\"step-content\">\r\n        <h2>Ëá™ÂÆö‰πâ‰Ω†ÁöÑÊúçÂä°Âô®</h2>\r\n        <p class=\"step-description\">Áªô‰Ω†ÁöÑÊúçÂä°Âô®‰∏Ä‰∏™Ë∫´‰ªΩ</p>\r\n        \r\n        <div class=\"form-group\">\r\n          <label>ÊúçÂä°Âô®ÂêçÁß∞</label>\r\n          <input \r\n            v-model=\"serverName\" \r\n            type=\"text\" \r\n            placeholder=\"ËæìÂÖ•ÊúçÂä°Âô®ÂêçÁß∞\"\r\n            class=\"form-input\"\r\n          >\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label>ÊúçÂä°Âô®ÂõæÊ†áÔºàÂèØÈÄâÔºâ</label>\r\n          <div class=\"server-icon-upload\" @click=\"triggerFileInput\">\r\n            <img \r\n              v-if=\"serverIcon\" \r\n              :src=\"serverIcon\" \r\n              alt=\"ÊúçÂä°Âô®ÂõæÊ†á\"\r\n              class=\"server-icon-preview\"\r\n            >\r\n            <div v-else class=\"upload-placeholder\">\r\n              <span class=\"upload-icon\">+</span>\r\n              <span>‰∏ä‰º†ÂõæÊ†á</span>\r\n            </div>\r\n          </div>\r\n          <input \r\n            type=\"file\" \r\n            ref=\"fileInput\"\r\n            accept=\"image/*\"\r\n            class=\"hidden\"\r\n            @change=\"handleIconUpload\"\r\n          >\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÂØºËà™ÊåâÈíÆ -->\r\n      <div class=\"modal-actions\">\r\n        <button \r\n          v-if=\"currentStep > 1\" \r\n          class=\"back-button\"\r\n          @click=\"currentStep--\"\r\n        >\r\n          ËøîÂõû\r\n        </button>\r\n        \r\n        <button \r\n          v-if=\"currentStep < 3\" \r\n          class=\"next-button\"\r\n          :disabled=\"!canProceed\"\r\n          @click=\"currentStep++\"\r\n        >\r\n          ÁªßÁª≠\r\n        </button>\r\n        \r\n        <button \r\n          v-else \r\n          class=\"create-button\"\r\n          :disabled=\"!canCreate\"\r\n          @click=\"createServer\"\r\n        >\r\n          ÂàõÂª∫ÊúçÂä°Âô®\r\n        </button>\r\n      </div>\r\n\r\n      <button class=\"close-button\" @click=\"closeModal\">√ó</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CreateServerModal',\r\n  props: {\r\n    isOpen: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      currentStep: 1,\r\n      selectedType: '',\r\n      selectedInterests: [],\r\n      serverName: '',\r\n      serverIcon: null,\r\n      // ÊúçÂä°Âô®Á±ªÂûãÈÄâÈ°π\r\n      serverTypes: [\r\n        {\r\n          id: 'gaming',\r\n          name: 'Ê∏∏ÊàèÁ§æÂå∫',\r\n          description: 'ÂàõÂª∫‰∏Ä‰∏™Ê∏∏ÊàèÁé©ÂÆ∂ÁöÑ‰∫§ÊµÅÁ§æÂå∫',\r\n          icon: 'üéÆ'\r\n        },\r\n        {\r\n          id: 'study',\r\n          name: 'Â≠¶‰π†Â∞èÁªÑ',\r\n          description: 'ÂàõÂª∫‰∏Ä‰∏™Â≠¶‰π†‰∫§ÊµÅÁöÑÁ©∫Èó¥',\r\n          icon: 'üìö'\r\n        },\r\n        {\r\n          id: 'social',\r\n          name: 'Á§æ‰∫§ËÅäÂ§©',\r\n          description: 'ÂàõÂª∫‰∏Ä‰∏™ÊúãÂèãÈó¥ÁöÑËÅäÂ§©Á©∫Èó¥',\r\n          icon: 'üí¨'\r\n        },\r\n        {\r\n          id: 'other',\r\n          name: 'ÂÖ∂‰ªñ',\r\n          description: 'ÂàõÂª∫‰∏Ä‰∏™Ëá™ÂÆö‰πâÁ±ªÂûãÁöÑÊúçÂä°Âô®',\r\n          icon: 'üåü'\r\n        }\r\n      ],\r\n      // ÂÖ¥Ë∂£Ê†áÁ≠æÈÄâÈ°π\r\n      interests: [\r\n        { id: 'games', name: 'Ê∏∏Êàè', icon: 'üéÆ' },\r\n        { id: 'music', name: 'Èü≥‰πê', icon: 'üéµ' },\r\n        { id: 'art', name: 'Ëâ∫ÊúØ', icon: 'üé®' },\r\n        { id: 'tech', name: 'ÁßëÊäÄ', icon: 'üíª' },\r\n        { id: 'sports', name: 'ËøêÂä®', icon: '‚öΩ' },\r\n        { id: 'food', name: 'ÁæéÈ£ü', icon: 'üçú' },\r\n        { id: 'travel', name: 'ÊóÖË°å', icon: '‚úàÔ∏è' },\r\n        { id: 'movies', name: 'ÁîµÂΩ±', icon: 'üé¨' },\r\n        { id: 'books', name: 'ËØª‰π¶', icon: 'üìö' },\r\n        { id: 'pets', name: 'ÂÆ†Áâ©', icon: 'üê±' },\r\n        { id: 'fashion', name: 'Êó∂Â∞ö', icon: 'üëó' },\r\n        { id: 'education', name: 'ÊïôËÇ≤', icon: 'üìñ' }\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    // Âà§Êñ≠ÊòØÂê¶ÂèØ‰ª•ËøõË°å‰∏ã‰∏ÄÊ≠•\r\n    canProceed() {\r\n      switch (this.currentStep) {\r\n        case 1:\r\n          return !!this.selectedType\r\n        case 2:\r\n          return this.selectedInterests.length > 0\r\n        default:\r\n          return true\r\n      }\r\n    },\r\n    // Âà§Êñ≠ÊòØÂê¶ÂèØ‰ª•ÂàõÂª∫ÊúçÂä°Âô®\r\n    canCreate() {\r\n      return this.serverName.trim().length > 0\r\n    }\r\n  },\r\n  methods: {\r\n    // ÂÖ≥Èó≠Ê®°ÊÄÅÊ°Ü\r\n    closeModal() {\r\n      this.$emit('close')\r\n      this.resetForm()\r\n    },\r\n    \r\n    // ÂàáÊç¢ÂÖ¥Ë∂£Ê†áÁ≠æÁöÑÈÄâ‰∏≠Áä∂ÊÄÅ\r\n    toggleInterest(interestId) {\r\n      const index = this.selectedInterests.indexOf(interestId)\r\n      if (index === -1) {\r\n        if (this.selectedInterests.length < 5) { // ÊúÄÂ§öÈÄâÊã©5‰∏™ÂÖ¥Ë∂£\r\n          this.selectedInterests.push(interestId)\r\n        }\r\n      } else {\r\n        this.selectedInterests.splice(index, 1)\r\n      }\r\n    },\r\n    \r\n    // Ëß¶ÂèëÊñá‰ª∂ÈÄâÊã©\r\n    triggerFileInput() {\r\n      this.$refs.fileInput.click()\r\n    },\r\n    \r\n    // Â§ÑÁêÜÂõæÊ†á‰∏ä‰º†\r\n    async handleIconUpload(event) {\r\n      const file = event.target.files[0]\r\n      if (file) {\r\n        try {\r\n          const reader = new FileReader()\r\n          reader.onload = (e) => {\r\n            this.serverIcon = e.target.result\r\n          }\r\n          reader.readAsDataURL(file)\r\n        } catch (error) {\r\n          console.error('ÂõæÊ†á‰∏ä‰º†Â§±Ë¥•:', error)\r\n        }\r\n      }\r\n    },\r\n    \r\n    // ÂàõÂª∫ÊúçÂä°Âô®\r\n    createServer() {\r\n      const serverData = {\r\n        name: this.serverName,\r\n        type: this.selectedType,\r\n        interests: this.selectedInterests,\r\n        icon: this.serverIcon\r\n      }\r\n      \r\n      this.$emit('create', serverData)\r\n      this.closeModal()\r\n    },\r\n    \r\n    // ÈáçÁΩÆË°®Âçï\r\n    resetForm() {\r\n      this.currentStep = 1\r\n      this.selectedType = ''\r\n      this.selectedInterests = []\r\n      this.serverName = ''\r\n      this.serverIcon = null\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.create-server-modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1000;\r\n}\r\n\r\n.modal-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgba(0, 0, 0, 0.8);\r\n  backdrop-filter: blur(4px);\r\n}\r\n\r\n.modal-content {\r\n  position: relative;\r\n  width: 90%;\r\n  max-width: 480px;\r\n  background: var(--background-primary);\r\n  border-radius: 8px;\r\n  padding: 24px;\r\n  z-index: 1;\r\n}\r\n\r\n.step-indicator {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.step-dots {\r\n  display: flex;\r\n  gap: 8px;
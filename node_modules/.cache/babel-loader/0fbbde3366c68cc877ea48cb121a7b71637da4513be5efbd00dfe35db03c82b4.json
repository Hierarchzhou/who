{"ast":null,"code":"import { GiphyFetch } from '@giphy/js-fetch-api';\n\n// 这里需要替换成你的 GIPHY API key\nconst gf = new GiphyFetch('YOUR_GIPHY_API_KEY');\nexport default {\n  name: 'StickerPicker',\n  data() {\n    return {\n      searchQuery: '',\n      stickers: [],\n      loading: false,\n      currentCategory: 'trending',\n      categories: [{\n        id: 'trending',\n        name: '热门'\n      }, {\n        id: 'reactions',\n        name: '反应'\n      }, {\n        id: 'memes',\n        name: '梗图'\n      }, {\n        id: 'anime',\n        name: '动漫'\n      }, {\n        id: 'cute',\n        name: '可爱'\n      }],\n      searchTimeout: null\n    };\n  },\n  methods: {\n    async fetchStickers(options = {}) {\n      this.loading = true;\n      try {\n        let response;\n        if (this.searchQuery) {\n          response = await gf.search(this.searchQuery, {\n            ...options,\n            type: 'stickers',\n            limit: 50\n          });\n        } else {\n          response = await gf.trending({\n            ...options,\n            type: 'stickers',\n            limit: 50\n          });\n        }\n        this.stickers = response.data;\n      } catch (error) {\n        console.error('Error fetching stickers:', error);\n      }\n      this.loading = false;\n    },\n    handleSearch() {\n      if (this.searchTimeout) {\n        clearTimeout(this.searchTimeout);\n      }\n      this.searchTimeout = setTimeout(() => {\n        this.fetchStickers();\n      }, 300);\n    },\n    selectCategory(category) {\n      this.currentCategory = category.id;\n      this.searchQuery = category.id === 'trending' ? '' : category.name;\n      this.fetchStickers();\n    },\n    selectSticker(sticker) {\n      this.$emit('select', {\n        id: sticker.id,\n        url: sticker.images.fixed_height.url,\n        title: sticker.title\n      });\n    }\n  },\n  mounted() {\n    this.fetchStickers();\n  }\n};","map":{"version":3,"names":["GiphyFetch","gf","name","data","searchQuery","stickers","loading","currentCategory","categories","id","searchTimeout","methods","fetchStickers","options","response","search","type","limit","trending","error","console","handleSearch","clearTimeout","setTimeout","selectCategory","category","selectSticker","sticker","$emit","url","images","fixed_height","title","mounted"],"sources":["C:\\Users\\Administrator\\Desktop\\discord-chat-app\\fack\\just-do-it\\src\\components\\chat\\StickerPicker.vue"],"sourcesContent":["<template>\r\n  <div class=\"sticker-picker\">\r\n    <!-- 搜索框 -->\r\n    <div class=\"search-container\">\r\n      <input \r\n        type=\"text\" \r\n        v-model=\"searchQuery\"\r\n        placeholder=\"搜索表情包...\"\r\n        @input=\"handleSearch\"\r\n        class=\"search-input\"\r\n      />\r\n    </div>\r\n\r\n    <!-- 分类标签 -->\r\n    <div class=\"categories\">\r\n      <button \r\n        v-for=\"category in categories\" \r\n        :key=\"category.id\"\r\n        @click=\"selectCategory(category)\"\r\n        :class=\"['category-btn', { active: currentCategory === category.id }]\"\r\n      >\r\n        {{ category.name }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- 表情包网格 -->\r\n    <div class=\"stickers-grid\" ref=\"gridContainer\">\r\n      <div v-if=\"loading\" class=\"loading\">加载中...</div>\r\n      <template v-else>\r\n        <div \r\n          v-for=\"sticker in stickers\" \r\n          :key=\"sticker.id\" \r\n          class=\"sticker-item\"\r\n          @click=\"selectSticker(sticker)\"\r\n        >\r\n          <img \r\n            :src=\"sticker.images.fixed_height_small.url\" \r\n            :alt=\"sticker.title\"\r\n            loading=\"lazy\"\r\n          />\r\n        </div>\r\n      </template>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GiphyFetch } from '@giphy/js-fetch-api'\r\n\r\n// 这里需要替换成你的 GIPHY API key\r\nconst gf = new GiphyFetch('YOUR_GIPHY_API_KEY')\r\n\r\nexport default {\r\n  name: 'StickerPicker',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      stickers: [],\r\n      loading: false,\r\n      currentCategory: 'trending',\r\n      categories: [\r\n        { id: 'trending', name: '热门' },\r\n        { id: 'reactions', name: '反应' },\r\n        { id: 'memes', name: '梗图' },\r\n        { id: 'anime', name: '动漫' },\r\n        { id: 'cute', name: '可爱' }\r\n      ],\r\n      searchTimeout: null\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchStickers(options = {}) {\r\n      this.loading = true\r\n      try {\r\n        let response\r\n        if (this.searchQuery) {\r\n          response = await gf.search(this.searchQuery, {\r\n            ...options,\r\n            type: 'stickers',\r\n            limit: 50\r\n          })\r\n        } else {\r\n          response = await gf.trending({\r\n            ...options,\r\n            type: 'stickers',\r\n            limit: 50\r\n          })\r\n        }\r\n        this.stickers = response.data\r\n      } catch (error) {\r\n        console.error('Error fetching stickers:', error)\r\n      }\r\n      this.loading = false\r\n    },\r\n\r\n    handleSearch() {\r\n      if (this.searchTimeout) {\r\n        clearTimeout(this.searchTimeout)\r\n      }\r\n      this.searchTimeout = setTimeout(() => {\r\n        this.fetchStickers()\r\n      }, 300)\r\n    },\r\n\r\n    selectCategory(category) {\r\n      this.currentCategory = category.id\r\n      this.searchQuery = category.id === 'trending' ? '' : category.name\r\n      this.fetchStickers()\r\n    },\r\n\r\n    selectSticker(sticker) {\r\n      this.$emit('select', {\r\n        id: sticker.id,\r\n        url: sticker.images.fixed_height.url,\r\n        title: sticker.title\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchStickers()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sticker-picker {\r\n  width: 100%;\r\n  height: 400px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background: var(--background-primary);\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r\n\r\n.search-container {\r\n  padding: 12px;\r\n  border-bottom: 1px solid var(--border-color);\r\n}\r\n\r\n.search-input {\r\n  width: 100%;\r\n  padding: 8px 12px;\r\n  border-radius: 4px;\r\n  border: 1px solid var(--border-color);\r\n  background: var(--background-secondary);\r\n  color: var(--text-normal);\r\n  font-size: 14px;\r\n}\r\n\r\n.categories {\r\n  padding: 8px;\r\n  display: flex;\r\n  gap: 8px;\r\n  overflow-x: auto;\r\n  border-bottom: 1px solid var(--border-color);\r\n}\r\n\r\n.category-btn {\r\n  padding: 6px 12px;\r\n  border: none;\r\n  border-radius: 16px;\r\n  background: var(--background-secondary);\r\n  color: var(--text-normal);\r\n  cursor: pointer;\r\n  white-space: nowrap;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.category-btn.active {\r\n  background: var(--primary-color);\r\n  color: white;\r\n}\r\n\r\n.stickers-grid {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding: 12px;\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));\r\n  gap: 8px;\r\n}\r\n\r\n.sticker-item {\r\n  aspect-ratio: 1;\r\n  border-radius: 4px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  transition: transform 0.2s ease;\r\n  background: var(--background-secondary);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.sticker-item:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.sticker-item img {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n  object-fit: contain;\r\n}\r\n\r\n.loading {\r\n  grid-column: 1 / -1;\r\n  text-align: center;\r\n  padding: 20px;\r\n  color: var(--text-muted);\r\n}\r\n\r\n/* 滚动条样式 */\r\n::-webkit-scrollbar {\r\n  width: 8px;\r\n  height: 8px;\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n  background: var(--background-secondary);\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n  background: var(--background-modifier-hover);\r\n  border-radius: 4px;\r\n}\r\n\r\n::-webkit-scrollbar-thumb:hover {\r\n  background: var(--background-modifier-active);\r\n}\r\n</style> "],"mappings":"AA+CA,SAASA,UAAS,QAAS,qBAAoB;;AAE/C;AACA,MAAMC,EAAC,GAAI,IAAID,UAAU,CAAC,oBAAoB;AAE9C,eAAe;EACbE,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,KAAK;MACdC,eAAe,EAAE,UAAU;MAC3BC,UAAU,EAAE,CACV;QAAEC,EAAE,EAAE,UAAU;QAAEP,IAAI,EAAE;MAAK,CAAC,EAC9B;QAAEO,EAAE,EAAE,WAAW;QAAEP,IAAI,EAAE;MAAK,CAAC,EAC/B;QAAEO,EAAE,EAAE,OAAO;QAAEP,IAAI,EAAE;MAAK,CAAC,EAC3B;QAAEO,EAAE,EAAE,OAAO;QAAEP,IAAI,EAAE;MAAK,CAAC,EAC3B;QAAEO,EAAE,EAAE,MAAM;QAAEP,IAAI,EAAE;MAAK,EAC1B;MACDQ,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAACC,OAAM,GAAI,CAAC,CAAC,EAAE;MAChC,IAAI,CAACP,OAAM,GAAI,IAAG;MAClB,IAAI;QACF,IAAIQ,QAAO;QACX,IAAI,IAAI,CAACV,WAAW,EAAE;UACpBU,QAAO,GAAI,MAAMb,EAAE,CAACc,MAAM,CAAC,IAAI,CAACX,WAAW,EAAE;YAC3C,GAAGS,OAAO;YACVG,IAAI,EAAE,UAAU;YAChBC,KAAK,EAAE;UACT,CAAC;QACH,OAAO;UACLH,QAAO,GAAI,MAAMb,EAAE,CAACiB,QAAQ,CAAC;YAC3B,GAAGL,OAAO;YACVG,IAAI,EAAE,UAAU;YAChBC,KAAK,EAAE;UACT,CAAC;QACH;QACA,IAAI,CAACZ,QAAO,GAAIS,QAAQ,CAACX,IAAG;MAC9B,EAAE,OAAOgB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK;MACjD;MACA,IAAI,CAACb,OAAM,GAAI,KAAI;IACrB,CAAC;IAEDe,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACX,aAAa,EAAE;QACtBY,YAAY,CAAC,IAAI,CAACZ,aAAa;MACjC;MACA,IAAI,CAACA,aAAY,GAAIa,UAAU,CAAC,MAAM;QACpC,IAAI,CAACX,aAAa,CAAC;MACrB,CAAC,EAAE,GAAG;IACR,CAAC;IAEDY,cAAcA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAAClB,eAAc,GAAIkB,QAAQ,CAAChB,EAAC;MACjC,IAAI,CAACL,WAAU,GAAIqB,QAAQ,CAAChB,EAAC,KAAM,UAAS,GAAI,EAAC,GAAIgB,QAAQ,CAACvB,IAAG;MACjE,IAAI,CAACU,aAAa,CAAC;IACrB,CAAC;IAEDc,aAAaA,CAACC,OAAO,EAAE;MACrB,IAAI,CAACC,KAAK,CAAC,QAAQ,EAAE;QACnBnB,EAAE,EAAEkB,OAAO,CAAClB,EAAE;QACdoB,GAAG,EAAEF,OAAO,CAACG,MAAM,CAACC,YAAY,CAACF,GAAG;QACpCG,KAAK,EAAEL,OAAO,CAACK;MACjB,CAAC;IACH;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACrB,aAAa,CAAC;EACrB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
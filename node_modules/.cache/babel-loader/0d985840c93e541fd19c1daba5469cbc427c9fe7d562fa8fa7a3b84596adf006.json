{"ast":null,"code":"import { GiphyFetch } from '@giphy/js-fetch-api';\nimport { GIPHY_API_KEY } from '@/config/giphy';\n\n// 初始化 GIPHY API 客户端\nconst gf = new GiphyFetch(GIPHY_API_KEY);\nexport default {\n  name: 'StickerMessage',\n  props: {\n    stickerId: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      stickerUrl: null\n    };\n  },\n  methods: {\n    async fetchSticker() {\n      try {\n        const {\n          data\n        } = await gf.gif(this.stickerId);\n        this.stickerUrl = data.images.fixed_height.url;\n      } catch (error) {\n        console.error('Error fetching sticker:', error);\n      }\n    },\n    onLoad() {\n      this.$emit('loaded');\n    }\n  },\n  created() {\n    this.fetchSticker();\n  }\n};","map":{"version":3,"names":["GiphyFetch","GIPHY_API_KEY","gf","name","props","stickerId","type","String","required","data","stickerUrl","methods","fetchSticker","gif","images","fixed_height","url","error","console","onLoad","$emit","created"],"sources":["C:\\Users\\Administrator\\Desktop\\discord-chat-app\\fack\\just-do-it\\src\\components\\chat\\StickerMessage.vue"],"sourcesContent":["<template>\r\n  <div class=\"sticker-message\">\r\n    <img \r\n      :src=\"stickerUrl\" \r\n      :alt=\"stickerId\"\r\n      @load=\"onLoad\"\r\n      v-if=\"stickerUrl\"\r\n      class=\"sticker-image\"\r\n    />\r\n    <div v-else class=\"sticker-loading\">\r\n      加载中...\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GiphyFetch } from '@giphy/js-fetch-api'\r\nimport { GIPHY_API_KEY } from '@/config/giphy'\r\n\r\n// 初始化 GIPHY API 客户端\r\nconst gf = new GiphyFetch(GIPHY_API_KEY)\r\n\r\nexport default {\r\n  name: 'StickerMessage',\r\n  props: {\r\n    stickerId: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      stickerUrl: null\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchSticker() {\r\n      try {\r\n        const { data } = await gf.g
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport UserNameInput from './UserNameInput.vue';\nimport MessageList from './MessageList.vue';\nimport MessageInput from './MessageInput.vue';\nimport ThemeSelector from './ThemeSelector.vue';\nimport OnlineUsers from './OnlineUsers.vue';\nimport AvatarSelector from './AvatarSelector.vue';\nimport axios from 'axios';\nimport { ref, onMounted, onUnmounted } from 'vue';\nexport default {\n  name: 'ChatContainer',\n  components: {\n    UserNameInput,\n    MessageList,\n    MessageInput,\n    ThemeSelector,\n    OnlineUsers,\n    AvatarSelector\n  },\n  data() {\n    return {\n      username: '',\n      // 用户昵称\n      userAvatar: '',\n      // 用户头像\n      messages: [],\n      // 消息列表\n      onlineUsers: [],\n      // 在线用户列表\n      apiUrl: 'http://localhost:3000/api/messages',\n      // 消息API地址\n      usersApiUrl: 'http://localhost:3000/api/users',\n      // 用户API地址\n      pollingInterval: null,\n      // 用于轮询消息的计时器\n      onlinePollingInterval: null,\n      // 用于轮询在线用户的计时器\n      currentTheme: localStorage.getItem('chat-theme') || 'discord' // 当前主题\n    };\n  },\n  created() {\n    // 检查是否有保存的用户信息\n    const savedUsername = localStorage.getItem('chat-username');\n    const savedAvatar = localStorage.getItem('chat-avatar');\n    if (savedUsername) {\n      this.username = savedUsername;\n      this.userAvatar = savedAvatar || '';\n      this.fetchMessages(); // 获取消息\n      this.fetchOnlineUsers(); // 获取在线用户\n      this.startPolling(); // 开始轮询获取新消息和在线用户\n      this.setUserOnline(); // 设置用户在线状态\n    }\n\n    // 添加页面关闭事件监听器，用于设置用户离线状态\n    window.addEventListener('beforeunload', this.setUserOffline);\n  },\n  beforeUnmount() {\n    // 组件销毁前清除轮询和事件监听器\n    this.stopPolling();\n    window.removeEventListener('beforeunload', this.setUserOffline);\n    this.setUserOffline(); // 设置用户离线状态\n  },\n  methods: {\n    // 设置用户名和头像\n    setUsername(userData) {\n      this.username = userData.username; // 设置用户名\n      this.userAvatar = userData.avatar || ''; // 设置头像\n\n      // 保存用户信息到本地存储\n      localStorage.setItem('chat-username', this.username);\n      if (this.userAvatar) {\n        localStorage.setItem('chat-avatar', this.userAvatar);\n      }\n      this.fetchMessages(); // 获取消息\n      this.fetchOnlineUsers(); // 获取在线用户\n      this.startPolling(); // 开始轮询获取新消息和在线用户\n      this.setUserOnline(); // 设置用户在线状态\n    },\n    // 更新用户头像\n    async updateAvatar(avatarUrl) {\n      this.userAvatar = avatarUrl; // 更新头像\n      localStorage.setItem('chat-avatar', avatarUrl); // 保存头像到本地存储\n\n      try {\n        // 更新服务器上的头像\n        await axios.post(`${this.usersApiUrl}/avatar`, {\n          username: this.username,\n          avatar: avatarUrl\n        });\n      } catch (error) {\n        console.error('更新头像失败:', error);\n      }\n    },\n    // 设置用户在线状态\n    async setUserOnline() {\n      try {\n        await axios.post(`${this.usersApiUrl}/online`, {\n          username: this.username,\n          avatar: this.userAvatar\n        });\n      } catch (error) {\n        console.error('设置用户在线状态失败:', error);\n      }\n    },\n    // 设置用户离线状态\n    async setUserOffline() {\n      if (this.username) {\n        try {\n          await axios.post(`${this.usersApiUrl}/offline`, {\n            username: this.username\n          });\n        } catch (error) {\n          console.error('设置用户离线状态失败:', error);\n        }\n      }\n    },\n    // 获取消息列表\n    async fetchMessages() {\n      try {\n        const response = await axios.get(this.apiUrl);\n        this.messages = response.data;\n      } catch (error) {\n        console.error('获取消息失败:', error);\n        // 开发阶段如果API未就绪，使用模拟数据\n        if (this.messages.length === 0) {\n          this.messages = [{\n            id: 1,\n            user: 'System',\n            content: '欢迎来到聊天室!',\n            timestamp: new Date().toISOString(),\n            avatar: 'https://ui-avatars.com/api/?name=System&background=ED4245&color=fff'\n          }];\n        }\n      }\n    },\n    // 获取在线用户列表\n    async fetchOnlineUsers() {\n      try {\n        const response = await axios.get(`${this.usersApiUrl}/online`);\n        this.onlineUsers = response.data;\n      } catch (error) {\n        console.error('获取在线用户失败:', error);\n      }\n    },\n    // 发送消息\n    async sendMessage(content) {\n      try {\n        const newMessage = {\n          user: this.username,\n          content: content,\n          avatar: this.userAvatar\n        };\n\n        // 发送消息到API\n        const response = await axios.post(this.apiUrl, newMessage);\n\n        // 将新消息添加到消息列表\n        this.messages.push(response.data);\n      } catch (error) {\n        console.error('发送消息失败:', error);\n\n        // 开发阶段如果API未就绪，模拟消息发送\n        const mockMessage = {\n          id: Date.now(),\n          user: this.username,\n          content: content,\n          avatar: this.userAvatar,\n          timestamp: new Date().toISOString()\n        };\n        this.messages.push(mockMessage);\n      }\n\n      // 更新用户在线状态\n      this.setUserOnline();\n    },\n    // 开始轮询获取新消息和在线用户\n    startPolling() {\n      // 轮询消息\n      this.pollingInterval = setInterval(() => {\n        this.fetchMessages();\n      }, 3000); // 每3秒轮询一次\n\n      // 轮询在线用户\n      this.onlinePollingInterval = setInterval(() => {\n        this.fetchOnlineUsers();\n        this.setUserOnline(); // 保持用户在线状态\n      }, 30000); // 每30秒轮询一次\n    },\n    // 停止轮询\n    stopPolling() {\n      if (this.pollingInterval) {\n        clearInterval(this.pollingInterval);\n        this.pollingInterval = null;\n      }\n      if (this.onlinePollingInterval) {\n        clearInterval(this.onlinePollingInterval);\n        this.onlinePollingInterval = null;\n      }\n    },\n    // 主题变更处理函数\n    onThemeChanged(theme) {\n      this.currentTheme = theme;\n    }\n  }\n};","map":{"version":3,"names":["UserNameInput","MessageList","MessageInput","ThemeSelector","OnlineUsers","AvatarSelector","axios","ref","onMounted","onUnmounted","name","components","data","username","userAvatar","messages","onlineUsers","apiUrl","usersApiUrl","pollingInterval","onlinePollingInterval","currentTheme","localStorage","getItem","created","savedUsername","savedAvatar","fetchMessages","fetchOnlineUsers","startPolling","setUserOnline","window","addEventListener","setUserOffline","beforeUnmount","stopPolling","removeEventListener","methods","setUsername","userData","avatar","setItem","updateAvatar","avatarUrl","post","error","console","response","get","length","id","user","content","timestamp","Date","toISOString","sendMessage","newMessage","push","mockMessage","now","setInterval","clearInterval","onThemeChanged","theme"],"sources":["C:\\Users\\Administrator\\Desktop\\discord-chat-app\\just-do-it\\src\\components\\chat\\ChatContainer.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-container\">\r\n    <!-- 用户名输入界面，当用户未设置昵称时显示 -->\r\n    <UserNameInput \r\n      v-if=\"!username\" \r\n      @set-username=\"setUsername\" \r\n    />\r\n    \r\n    <!-- 聊天界面，当用户已设置昵称时显示 -->\r\n    <div v-else class=\"chat-interface\">\r\n      <header class=\"chat-header\">\r\n        <h1 class=\"chat-title\">Discord风格聊天室</h1>\r\n        <div class=\"user-info-container\">\r\n          <ThemeSelector @theme-changed=\"onThemeChanged\" />\r\n          <div class=\"user-info\">\r\n            <AvatarSelector \r\n              :username=\"username\" \r\n              :initial-avatar=\"userAvatar\"\r\n              @avatar-selected=\"updateAvatar\" \r\n            />\r\n            <span class=\"current-username\">{{ username }}</span>\r\n          </div>\r\n        </div>\r\n      </header>\r\n      \r\n      <div class=\"chat-main\">\r\n        <MessageList \r\n          :messages=\"messages\" \r\n          :currentUsername=\"username\" \r\n          ref=\"messageList\" \r\n        />\r\n        <OnlineUsers \r\n          :onlineUsers=\"onlineUsers\" \r\n          :currentUsername=\"username\" \r\n        />\r\n      </div>\r\n      \r\n      <MessageInput @send-message=\"sendMessage\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserNameInput from './UserNameInput.vue';\r\nimport MessageList from './MessageList.vue';\r\nimport MessageInput from './MessageInput.vue';\r\nimport ThemeSelector from './ThemeSelector.vue';\r\nimport OnlineUsers from './OnlineUsers.vue';\r\nimport AvatarSelector from './AvatarSelector.vue';\r\nimport axios from 'axios';\r\nimport { ref, onMounted, onUnmounted } from 'vue';\r\n\r\nexport default {\r\n  name: 'ChatContainer',\r\n  components: {\r\n    UserNameInput,\r\n    MessageList,\r\n    MessageInput,\r\n    ThemeSelector,\r\n    OnlineUsers,\r\n    AvatarSelector\r\n  },\r\n  data() {\r\n    return {\r\n      username: '', // 用户昵称\r\n      userAvatar: '', // 用户头像\r\n      messages: [], // 消息列表\r\n      onlineUsers: [], // 在线用户列表\r\n      apiUrl: 'http://localhost:3000/api/messages', // 消息API地址\r\n      usersApiUrl: 'http://localhost:3000/api/users', // 用户API地址\r\n      pollingInterval: null, // 用于轮询消息的计时器\r\n      onlinePollingInterval: null, // 用于轮询在线用户的计时器\r\n      currentTheme: localStorage.getItem('chat-theme') || 'discord' // 当前主题\r\n    }\r\n  },\r\n  created() {\r\n    // 检查是否有保存的用户信息\r\n    const savedUsername = localStorage.getItem('chat-username');\r\n    const savedAvatar = localStorage.getItem('chat-avatar');\r\n    \r\n    if (savedUsername) {\r\n      this.username = savedUsername;\r\n      this.userAvatar = savedAvatar || '';\r\n      \r\n      this.fetchMessages(); // 获取消息\r\n      this.fetchOnlineUsers(); // 获取在线用户\r\n      this.startPolling(); // 开始轮询获取新消息和在线用户\r\n      this.setUserOnline(); // 设置用户在线状态\r\n    }\r\n    \r\n    // 添加页面关闭事件监听器，用于设置用户离线状态\r\n    window.addEventListener('beforeunload', this.setUserOffline);\r\n  },\r\n  beforeUnmount() {\r\n    // 组件销毁前清除轮询和事件监听器\r\n    this.stopPolling();\r\n    window.removeEventListener('beforeunload', this.setUserOffline);\r\n    this.setUserOffline(); // 设置用户离线状态\r\n  },\r\n  methods: {\r\n    // 设置用户名和头像\r\n    setUsername(userData) {\r\n      this.username = userData.username; // 设置用户名\r\n      this.userAvatar = userData.avatar || ''; // 设置头像\r\n      \r\n      // 保存用户信息到本地存储\r\n      localStorage.setItem('chat-username', this.username);\r\n      if (this.userAvatar) {\r\n        localStorage.setItem('chat-avatar', this.userAvatar);\r\n      }\r\n      \r\n      this.fetchMessages(); // 获取消息\r\n      this.fetchOnlineUsers(); // 获取在线用户\r\n      this.startPolling(); // 开始轮询获取新消息和在线用户\r\n      this.setUserOnline(); // 设置用户在线状态\r\n    },\r\n    \r\n    // 更新用户头像\r\n    async updateAvatar(avatarUrl) {\r\n      this.userAvatar = avatarUrl; // 更新头像\r\n      localStorage.setItem('chat-avatar', avatarUrl); // 保存头像到本地存储\r\n      \r\n      try {\r\n        // 更新服务器上的头像\r\n        await axios.post(`${this.usersApiUrl}/avatar`, {\r\n          username: this.username,\r\n          avatar: avatarUrl\r\n        });\r\n      } catch (error) {\r\n        console.error('更新头像失败:', error);\r\n      }\r\n    },\r\n    \r\n    // 设置用户在线状态\r\n    async setUserOnline() {\r\n      try {\r\n        await axios.post(`${this.usersApiUrl}/online`, {\r\n          username: this.username,\r\n          avatar: this.userAvatar\r\n        });\r\n      } catch (error) {\r\n        console.error('设置用户在线状态失败:', error);\r\n      }\r\n    },\r\n    \r\n    // 设置用户离线状态\r\n    async setUserOffline() {\r\n      if (this.username) {\r\n        try {\r\n          await axios.post(`${this.usersApiUrl}/offline`, {\r\n            username: this.username\r\n          });\r\n        } catch (error) {\r\n          console.error('设置用户离线状态失败:', error);\r\n        }\r\n      }\r\n    },\r\n    \r\n    // 获取消息列表\r\n    async fetchMessages() {\r\n      try {\r\n        const response = await axios.get(this.apiUrl);\r\n        this.messages = response.data;\r\n      } catch (error) {\r\n        console.error('获取消息失败:', error);\r\n        // 开发阶段如果API未就绪，使用模拟数据\r\n        if (this.messages.length === 0) {\r\n          this.messages = [\r\n            { \r\n              id: 1, \r\n              user: 'System', \r\n              content: '欢迎来到聊天室!', \r\n              timestamp: new Date().toISOString(),\r\n              avatar: 'https://ui-avatars.com/api/?name=System&background=ED4245&color=fff'\r\n            }\r\n          ];\r\n        }\r\n      }\r\n    },\r\n    \r\n    // 获取在线用户列表\r\n    async fetchOnlineUsers() {\r\n      try {\r\n        const response = await axios.get(`${this.usersApiUrl}/online`);\r\n        this.onlineUsers = response.data;\r\n      } catch (error) {\r\n        console.error('获取在线用户失败:', error);\r\n      }\r\n    },\r\n    \r\n    // 发送消息\r\n    async sendMessage(content) {\r\n      try {\r\n        const newMessage = {\r\n          user: this.username,\r\n          content: content,\r\n          avatar: this.userAvatar\r\n        };\r\n        \r\n        // 发送消息到API\r\n        const response = await axios.post(this.apiUrl, newMessage);\r\n        \r\n        // 将新消息添加到消息列表\r\n        this.messages.push(response.data);\r\n      } catch (error) {\r\n        console.error('发送消息失败:', error);\r\n        \r\n        // 开发阶段如果API未就绪，模拟消息发送\r\n        const mockMessage = {\r\n          id: Date.now(),\r\n          user: this.username,\r\n          content: content,\r\n          avatar: this.userAvatar,\r\n          timestamp: new Date().toISOString()\r\n        };\r\n        this.messages.push(mockMessage);\r\n      }\r\n      \r\n      // 更新用户在线状态\r\n      this.setUserOnline();\r\n    },\r\n    \r\n    // 开始轮询获取新消息和在线用户\r\n    startPolling() {\r\n      // 轮询消息\r\n      this.pollingInterval = setInterval(() => {\r\n        this.fetchMessages();\r\n      }, 3000); // 每3秒轮询一次\r\n      \r\n      // 轮询在线用户\r\n      this.onlinePollingInterval = setInterval(() => {\r\n        this.fetchOnlineUsers();\r\n        this.setUserOnline(); // 保持用户在线状态\r\n      }, 30000); // 每30秒轮询一次\r\n    },\r\n    \r\n    // 停止轮询\r\n    stopPolling() {\r\n      if (this.pollingInterval) {\r\n        clearInterval(this.pollingInterval);\r\n        this.pollingInterval = null;\r\n      }\r\n      \r\n      if (this.onlinePollingInterval) {\r\n        clearInterval(this.onlinePollingInterval);\r\n        this.onlinePollingInterval = null;\r\n      }\r\n    },\r\n    \r\n    // 主题变更处理函数\r\n    onThemeChanged(theme) {\r\n      this.currentTheme = theme;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.chat-container {\r\n  height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: var(--chat-background); /* 使用主题变量 */\r\n  color: var(--text-color); /* 使用主题变量 */\r\n  position: relative;\r\n  z-index: 1;\r\n  max-width: 1400px;\r\n  margin: 0 auto;\r\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.chat-interface {\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100%;\r\n  backdrop-filter: blur(5px); /* 添加模糊效果 */\r\n}\r\n\r\n.chat-header {\r\n  padding: 1rem 1.5rem;\r\n  background-color: var(--primary-color); /* 使用主题变量 */\r\n  color: var(--header-color); /* 使用主题变量 */\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  box-shadow: var(--header-shadow);\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n\r\n.chat-title {\r\n  margin: 0;\r\n  font-size: 1.25rem;\r\n  font-weight: 600;\r\n  letter-spacing: 0.5px;\r\n}\r\n\r\n.user-info-container {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-left: 1rem;\r\n  background-color: rgba(255, 255, 255, 0.1);\r\n  padding: 0.5rem 0.75rem;\r\n  border-radius: 2rem;\r\n  backdrop-filter: blur(5px);\r\n}\r\n\r\n.current-username {\r\n  font-weight: bold;\r\n  margin-left: 0.5rem;\r\n}\r\n\r\n.chat-main {\r\n  display: flex;\r\n  flex: 1;\r\n  overflow: hidden;\r\n  position: relative;\r\n}\r\n\r\n/* 添加一个覆盖层，用于实现电影质感 */\r\n.chat-container::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  
{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { computed, ref } from 'vue';\nimport { useStore } from 'vuex';\nimport { getFallbackAvatarUrl } from '../../utils/avatarUtils';\nexport default {\n  __name: 'OnlineUsersSidebar',\n  props: {\n    onlineUsers: {\n      type: Array,\n      required: true,\n      default: () => []\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const props = __props;\n    const store = useStore();\n    const defaultAvatar = ref(getFallbackAvatarUrl());\n    const currentUsername = computed(() => store.getters['userProfile/username']);\n    const currentUserAvatar = computed(() => store.getters['userProfile/avatar']);\n    const otherUsers = computed(() => {\n      return props.onlineUsers.filter(user => user.username !== currentUsername.value);\n    });\n    const getCurrentUserAvatar = () => {\n      return currentUserAvatar.value || defaultAvatar.value;\n    };\n    const onAvatarError = event => {\n      if (event.target instanceof HTMLImageElement) {\n        event.target.src = defaultAvatar.value;\n      }\n    };\n    const __returned__ = {\n      props,\n      store,\n      defaultAvatar,\n      currentUsername,\n      currentUserAvatar,\n      otherUsers,\n      getCurrentUserAvatar,\n      onAvatarError,\n      computed,\n      ref,\n      get useStore() {\n        return useStore;\n      },\n      get getFallbackAvatarUrl() {\n        return getFallbackAvatarUrl;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","ref","useStore","getFallbackAvatarUrl","props","__props","store","defaultAvatar","currentUsername","getters","currentUserAvatar","otherUsers","onlineUsers","filter","user","username","value","getCurrentUserAvatar","onAvatarError","event","target","HTMLImageElement","src"],"sources":["C:/Users/Administrator/Desktop/discord-chat-app/fack/just-do-it/src/components/chat/OnlineUsersSidebar.vue"],"sourcesContent":["<template>\n  <div class=\"online-users-sidebar\">\n    <!-- 频道头部 -->\n    <div class=\"sidebar-header\">\n      <h3 class=\"sidebar-title\">在线用户 ({{ onlineUsers.length }})</h3>\n    </div>\n    \n    <!-- 用户列表 -->\n    <div class=\"users-list\">\n      <!-- 当前用户 -->\n      <div class=\"user-item current-user\" v-if=\"currentUsername\">\n        <div class=\"user-avatar-container\">\n          <img \n            :src=\"getCurrentUserAvatar()\" \n            alt=\"当前用户头像\" \n            class=\"user-avatar\"\n            @error=\"onAvatarError\"\n          />\n          <div class=\"user-status online\"></div>\n        </div>\n        <div class=\"user-info\">\n          <div class=\"user-name\">{{ currentUsername }} <span class=\"user-tag\">(我)</span></div>\n          <div class=\"user-status-text\">在线</div>\n        </div>\n      </div>\n      \n      <!-- 分隔线 -->\n      <div class=\"sidebar-divider\"></div>\n      \n      <!-- 其他在线用户 -->\n      <div \n        v-for=\"user in otherUsers\" \n        :key=\"user.username\"\n        class=\"user-item\"\n      >\n        <div class=\"user-avatar-container\">\n          <img \n            :src=\"user.avatar || defaultAvatar\" \n            alt=\"用户头像\" \n            class=\"user-avatar\"\n            @error=\"onAvatarError\"\n          />\n          <div class=\"user-status online\"></div>\n        </div>\n        <div class=\"user-info\">\n          <div class=\"user-name\">{{ user.username }}</div>\n          <div class=\"user-status-text\">在线</div>\n        </div>\n      </div>\n      \n      <!-- 无用户时的提示 -->\n      <div v-if=\"otherUsers.length === 0 && !currentUsername\" class=\"no-users\">\n        暂无其他用户在线\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { computed, ref } from 'vue';\nimport { useStore } from 'vuex';\nimport { getFallbackAvatarUrl } from '../../utils/avatarUtils';\n\nconst props = defineProps({\n  onlineUsers: {\n    type: Array,\n    required: true,\n    default: () => []\n  }\n});\n\nconst store = useStore();\nconst defaultAvatar = ref(getFallbackAvatarUrl());\n\nconst currentUsername = computed(() => store.getters['userProfile/username']);\nconst currentUserAvatar = computed(() => store.getters['userProfile/avatar']);\n\nconst otherUsers = computed(() => {\n  return props.onlineUsers.filter(user => user.username !== currentUsername.value);\n});\n\nconst getCurrentUserAvatar = () => {\n  return currentUserAvatar.value || defaultAvatar.value;\n};\n\nconst onAvatarError = (event) => {\n  if (event.target instanceof HTMLImageElement) {\n    event.target.src = defaultAvatar.value;\n  }\n};\n</script>\n\n<style scoped>\n.online-users-sidebar {\n  width: 240px; /* Discord频道列表宽度 */\n  height: 100vh;\n  background-color: #2f3136; /* Discord频道列表背景色 */\n  display: flex;\n  flex-direction: column;\n  flex-shrink: 0;\n  border-right: 1px solid #202225;\n}\n\n.sidebar-header {\n  padding: 16px;\n  border-bottom: 1px solid rgba(79, 84, 92, 0.48);\n  box-shadow: 0 1px 0 rgba(4, 4, 5, 0.2);\n}\n\n.sidebar-title {\n  color: #ffffff;\n  font-size: 16px;\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.users-list {\n  flex: 1;\n  overflow-y: auto;\n  padding: 8px 0;\n}\n\n.user-item {\n  display: flex;\n  align-items: center;\n  padding: 8px 16px;\n  cursor: pointer;\n  border-radius: 4px;\n  margin: 0 8px;\n  transition: background-color 0.2s ease;\n}\n\n.user-item:hover {\n  background-color: rgba(79, 84, 92, 0.16);\n}\n\n.user-item.current-user {\n  background-color: rgba(79, 84, 92, 0.32);\n}\n\n.user-avatar-container {\n  position: relative;\n  margin-right: 12px;\n}\n\n.user-avatar {\n  width: 32px;\n  height: 32px;\n  border-radius: 50%;\n  object-fit: cover;\n}\n\n.user-status {\n  position: absolute;\n  width: 10px;\n  height: 10px;\n  border-radius: 50%;\n  bottom: 0;\n  left: 0;\n  border: 2px solid #2f3136;\n}\n\n.user-status.online {\n  background-color: #43b581; /* Discord在线状态颜色 */\n}\n\n.user-info {\n  flex: 1;\n  overflow: hidden;\n}\n\n.user-name {\n  color: #ffffff;\n  font-size: 14px;\n  font-weight: 500;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n.user-tag {\n  color: #b9bbbe;\n  font-size: 12px;\n  font-weight: normal;\n}\n\n.user-status-text {\n  color: #b9bbbe;\n  font-size: 12px;\n  margin-top: 2px;\n}\n\n.sidebar-divider {\n  height: 1px;\n  background-color: rgba(79, 84, 92, 0.48);\n  margin: 8px 10px;\n}\n\n.no-users {\n  padding: 16px;\n  color: #b9bbbe;\n  font-style: italic;\n  text-align: center;\n}\n\n/* 滚动条样式 */\n.users-list::-webkit-scrollbar {\n  width: 4px;\n}\n\n.users-list::-webkit-scrollbar-thumb {\n  background-color: #202225;\n  border-radius: 2px;\n}\n\n.users-list::-webkit-scrollbar-track {\n  background-color: transparent;\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n  .online-users-sidebar {\n    width: 100%;\n    height: auto;\n    max-height: 200px;\n    border-right: none;\n    border-bottom: 1px solid #202225;\n  }\n  \n  .users-list {\n    display: flex;\n    flex-wrap: wrap;\n    padding: 8px;\n  }\n  \n  .user-item {\n    width: auto;\n    margin: 4px;\n  }\n}\n</style> "],"mappings":";;AA2DA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,oBAAoB,QAAQ,yBAAyB;;;;;;;;;;;;;;IAE9D,MAAMC,KAAK,GAAGC,OAMZ;IAEF,MAAMC,KAAK,GAAGJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,aAAa,GAAGN,GAAG,CAACE,oBAAoB,CAAC,CAAC,CAAC;IAEjD,MAAMK,eAAe,GAAGR,QAAQ,CAAC,MAAMM,KAAK,CAACG,OAAO,CAAC,sBAAsB,CAAC,CAAC;IAC7E,MAAMC,iBAAiB,GAAGV,QAAQ,CAAC,MAAMM,KAAK,CAACG,OAAO,CAAC,oBAAoB,CAAC,CAAC;IAE7E,MAAME,UAAU,GAAGX,QAAQ,CAAC,MAAM;MAChC,OAAOI,KAAK,CAACQ,WAAW,CAACC,MAAM,CAACC,IAAI,IAAIA,IAAI,CAACC,QAAQ,KAAKP,eAAe,CAACQ,KAAK,CAAC;IAClF,CAAC,CAAC;IAEF,MAAMC,oBAAoB,GAAGA,CAAA,KAAM;MACjC,OAAOP,iBAAiB,CAACM,KAAK,IAAIT,aAAa,CAACS,KAAK;IACvD,CAAC;IAED,MAA
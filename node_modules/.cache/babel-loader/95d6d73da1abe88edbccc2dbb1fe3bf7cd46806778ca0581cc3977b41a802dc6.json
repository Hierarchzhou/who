('Error fetching stickers:', error)\r\n      }\r\n      this.loading = false\r\n    },\r\n\r\n    handleSearch() {\r\n      if (this.searchTimeout) {\r\n        clearTimeout(this.searchTimeout)\r\n      }\r\n      this.searchTimeout = setTimeout(() => {\r\n        this.fetchStickers()\r\n      }, 300)\r\n    },\r\n\r\n    selectCategory(category) {\r\n      this.currentCategory = category.id\r\n      this.searchQuery = category.id === 'trending' ? '' : category.name\r\n      this.fetchStickers()\r\n    },\r\n\r\n    selectSticker(sticker) {\r\n      this.$emit('select', {\r\n        id: sticker.id,\r\n        url: sticker.images.fixed_height.url,\r\n        title: sticker.title\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchStickers()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sticker-picker {\r\n  width: 100%;\r\n  height: 400px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background: var(--background-primary);\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r\n\r\n.search-container {\r\n  padding: 12px;\r\n  border-bottom: 1px solid var(--border-color);\r\n}\r\n\r\n.search-input {\r\n  width: 100%;\r\n  padding: 8px 12px;\r\n  border-radius: 4px;\r\n  border: 1px solid var(--border-color);\r\n  background: var(--background-secondary);\r\n  color: var(--text-normal);\r\n  font-size: 14px;\r\n}\r\n\r\n.categories {\r\n  padding: 8px;\r\n  display: flex;\r\n  gap: 8px;\r\n  overflow-x: auto;\r\n  border-bottom: 1px solid var(--border-color);\r\n}\r\n\r\n.category-btn {\r\n  padding: 6px 12px;\r\n  border: none;\r\n  border-radius: 16px;\r\n  background: var(--background-secondary);\r\n  color: var(--text-normal);\r\n  cursor: pointer;\r\n  white-space: nowrap;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.category-btn.active {\r\n  background: var(--primary-color);\r\n  color: white;\r\n}\r\n\r\n.stickers-grid {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding: 12px;\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));\r\n  gap: 8px;\r\n}\r\n\r\n.sticker-item {\r\n  aspect-ratio: 1;\r\n  border-radius: 4px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  transition: transform 0.2s ease;\r\n  background: var(--background-secondary);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.sticker-item:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.sticker-item img {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n  object-fit: contain;\r\n}\r\n\r\n.loading {\r\n  grid-column: 1 / -1;\r\n  text-align: center;\r\n  padding: 20px;\r\n  color: var(--text-muted);\r\n}\r\n\r\n/* 滚动条样式 */\r\n::-webkit-scrollbar {\r\n  width: 8px;\r\n  height: 8px;\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n  background: var(--background-secondary);\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n  background: var(--background-modifier-hover);\r\n  border-radius: 4px;\r\n}\r\n\r\n::-webkit-scrollbar-thumb:hover {\r\n  background: var(--background-modifier-active);\r\n}\r\n</style> "],"mappings":";;EACOA,KAAK,EAAC;AAAgB;;EAEpBA,KAAK,EAAC;AAAkB;;EAWxBA,KAAK,EAAC;AAAY;mBAd3B;;EA0BSA,KAAK,EAAC,eAAe;EAACC,GAAG,EAAC;;;EA1BnCC,GAAA;EA2B0BF,KAAK,EAAC;;mBA3BhC;mBAAA;;uBACEG,mBAAA,CA0CM,OA1CNC,UA0CM,GAzCJC,mBAAA,SAAY,EACZC,mBAAA,CAQM,OARNC,UAQM,G,gBAPJD,mBAAA,CAME;IALAE,IAAI,EAAC,MAAM;IALnB,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAMiBC,KAAA,CAAAC,WAAW,GAAAF,MAAA;IACpBG,WAAW,EAAC,UAAU;IACrBC,OAAK,EAAAL,MAAA,QAAAA,MAAA,UAAAM,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;IACpBf,KAAK,EAAC;iEAHGW,KAAA,CAAAC,WAAW,E,KAOxBP,mBAAA,UAAa,EACbC,mBAAA,CASM,OATNY,UASM,I,kBARJf,mBAAA,CAOSgB,SAAA,QAtBfC,WAAA,CAgB2BT,KAAA,CAAAU,UAAU,EAAtBC,QAAQ;yBADjBnB,mBAAA,CAOS;MALND,GAAG,EAAEoB,QAAQ,CAACC,EAAE;MAChBC,OAAK,EAAAd,MAAA,IAAEM,QAAA,CAAAS,cAAc,CAACH,QAAQ;MAC9BtB,KAAK,EAnBd0B,eAAA;QAAAC,MAAA,EAmB2ChB,KAAA,CAAAiB,eAAe,KAAKN,QAAQ,CAACC;MAAE;wBAE/DD,QAAQ,CAACO,IAAI,gCArBxBC,UAAA;oCAyBIzB,mBAAA,WAAc,EACdC,mBAAA,CAgBM,OAhBNyB,UAgBM,GAfOpB,KAAA,CAAAqB,OAAO,I,cAAlB7B,mBAAA
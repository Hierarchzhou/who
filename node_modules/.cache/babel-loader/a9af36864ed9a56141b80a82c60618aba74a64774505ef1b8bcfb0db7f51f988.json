{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { Cropper } from 'vue-advanced-cropper';\nimport 'vue-advanced-cropper/dist/style.css';\nexport default {\n  name: 'AvatarSelector',\n  components: {\n    Cropper\n  },\n  props: {\n    username: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      selectedFile: null,\n      // 选中的文件对象\n      previewUrl: null,\n      // 预览图片的URL\n      showEditor: false,\n      // 是否显示编辑器\n      imageUrl: '',\n      // 裁剪器的图片URL\n      selectedPreset: null,\n      // 选中的预设头像ID\n      avatarStyles: [\n      // 头像风格列表\n      {\n        id: 'micah',\n        name: '简约',\n        options: {\n          baseColor: ['f9c9b6', 'ac6651'],\n          mouth: ['laughing', 'smile', 'smirk'],\n          hair: ['thick', 'short'],\n          hairColor: ['0e0e0e', '77311d', 'fc909f']\n        }\n      }, {\n        id: 'personas',\n        name: '人物',\n        options: {\n          backgroundColor: ['b6e3f4', 'c0aede', 'ffd5dc'],\n          clothing: ['dress', 'dressShirt', 'graphic', 'hoodie', 'shirt', 'tankTop'],\n          clothingColor: ['545454', '65c9ff', 'ffd35c', 'ff6b6b'],\n          face: ['smile', 'laugh', 'cute']\n        }\n      }, {\n        id: 'identicon',\n        name: '几何',\n        options: {\n          colors: ['ff5733', '4ecdc4', '45b7d1', 'ffd93d'],\n          size: 6\n        }\n      }, {\n        id: 'thumbs',\n        name: '简笔',\n        options: {\n          backgroundColor: ['ffd5dc', 'b6e3f4', 'c0aede'],\n          shapeColor: ['ff5733', '4ecdc4', '45b7d1'],\n          mood: ['happy', 'surprised', 'chill']\n        }\n      }]\n    };\n  },\n  methods: {\n    // 触发文件选择\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    // 处理文件选择\n    handleFileSelect(event) {\n      const file = event.target.files[0];\n      if (file && file.type.startsWith('image/')) {\n        this.selectedFile = file;\n        this.selectedPreset = null;\n        this.showEditor = true;\n        this.imageUrl = URL.createObjectURL(file);\n      }\n    },\n    // 处理裁剪变化\n    onChange({\n      canvas\n    }) {\n      if (canvas) {\n        this.previewUrl = canvas.toDataURL();\n      }\n    },\n    // 保存头像\n    async saveAvatar() {\n      if (this.previewUrl) {\n        this.showEditor = false;\n        this.$emit('avatar-selected', this.previewUrl);\n      }\n    },\n    // 取消编辑\n    cancelEdit() {\n      this.showEditor = false;\n      this.selectedFile = null;\n      this.imageUrl = '';\n      if (!this.selectedPreset) {\n        this.previewUrl = null;\n      }\n      if (this.$refs.fileInput) {\n        this.$refs.fileInput.value = '';\n      }\n    },\n    // 选择预设头像\n    selectPresetAvatar(style) {\n      this.selectedPreset = style.id;\n      this.selectedFile = null;\n      this.showEditor = false;\n      const avatarUrl = this.createAvatarUrl(style);\n      this.previewUrl = avatarUrl;\n      this.$emit('avatar-selected', avatarUrl);\n    },\n    // 创建头像URL\n    createAvatarUrl(style) {\n      try {\n        const seed = this.username || Math.random().toString(36).substring(7);\n        const options = {\n          ...style.options,\n          size: 128\n        };\n\n        // 使用最新的 DiceBear API\n        return `https://api.dicebear.com/7.x/${style.id}/svg?seed=${seed}&${this.serializeOptions(options)}`;\n      } catch (error) {\n        console.error(`创建头像URL时出错 (${style.id}):`, error);\n        return '';\n      }\n    },\n    // 序列化选项\n    serializeOptions(options) {\n      return Object.entries(options).map(([key, value]) => {\n        if (Array.isArray(value)) {\n          return `${key}=${value.join(',')}`;\n        }\n        return `${key}=${value}`;\n      }).join('&');\n    },\n    // 处理头像加载错误\n    handleAvatarError(styleId) {\n      console.error(`头像风格 ${styleId} 加载失败`);\n    }\n  }\n};","map":{"version":3,"names":["Cropper","name","components","props","username","type","String","default","data","selectedFile","previewUrl","showEditor","imageUrl","selectedPreset","avatarStyles","id","options","baseColor","mouth","hair","hairColor","backgroundColor","clothing","clothingColor","face","colors","size","shapeColor","mood","methods","triggerFileInput","$refs","fileInput","click","handleFileSelect","event","file","target","files","startsWith","URL","createObjectURL","onChange","canvas","toDataURL","saveAvatar","$emit","cancelEdit","value","selectPresetAvatar","style","avatarUrl","createAvatarUrl","seed","Math","random","toString","substring","serializeOptions","error","console","Object","entries","map","key","Array","isArray","join","handleAvatarError","styleId"],"sources":["C:\\Users\\Administrator\\Desktop\\discord-chat-app\\fack\\just-do-it\\src\\components\\chat\\AvatarSelector.vue"],"sourcesContent":["<template>\n  <div class=\"avatar-selector\">\n    <!-- 头像预览区域 -->\n    <div class=\"avatar-preview\" @click=\"triggerFileInput\">\n      <div v-if=\"previewUrl\" class=\"avatar-preview-container\">\n        <img :src=\"previewUrl\" alt=\"Avatar preview\" class=\"preview-image\" />\n        <div class=\"avatar-hover-effect\"></div>\n      </div>\n      <div v-else class=\"empty-preview\">\n        <div class=\"upload-icon\">\n          <span class=\"plus-horizontal\"></span>\n          <span class=\"plus-vertical\"></span>\n        </div>\n        <span>更换头像</span>\n      </div>\n      <input\n        type=\"file\"\n        ref=\"fileInput\"\n        @change=\"handleFileSelect\"\n        accept=\"image/*\"\n        style=\"display: none\"\n      />\n    </div>\n\n    <!-- 头像编辑器模态框 -->\n    <div v-if=\"showEditor\" class=\"editor-modal\">\n      <div class=\"editor-container\">\n        <Cropper\n          ref=\"cropper\"\n          class=\"cropper\"\n          :src=\"imageUrl\"\n          :stencil-props=\"{\n            aspectRatio: 1\n          }\"\n          @change=\"onChange\"\n        />\n        <div class=\"editor-controls\">\n          <div class=\"editor-buttons\">\n            <button @click=\"saveAvatar\" class=\"save-button\">保存</button>\n            <button @click=\"cancelEdit\" class=\"cancel-button\">取消</button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 预设头像选择区域 -->\n    <div class=\"preset-avatars\">\n      <h3>选择头像风格</h3>\n      <div class=\"avatar-grid\">\n        <div\n          v-for=\"style in avatarStyles\"\n          :key=\"style.id\"\n          class=\"preset-avatar-wrapper\"\n        >\n          <div\n            class=\"preset-avatar\"\n            :class=\"[\n              `avatar-style-${style.id}`,\n              { selected: selectedPreset === style.id }\n            ]\"\n            @click=\"selectPresetAvatar(style)\"\n          >\n            <img\n              :src=\"createAvatarUrl(style)\"\n              :alt=\"style.name\"\n              class=\"preset-avatar-image\"\n              @error=\"handleAvatarError(style.id)\"\n            />\n            <div class=\"avatar-hover-effect\"></div>\n            <span class=\"preset-avatar-name\">{{ style.name }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Cropper } from 'vue-advanced-cropper'\nimport 'vue-advanced-cropper/dist/style.css'\n\nexport default {\n  name: 'AvatarSelector',\n  components: {\n    Cropper\n  },\n  props: {\n    username: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      selectedFile: null,        // 选中的文件对象\n      previewUrl: null,         // 预览图片的URL\n      showEditor: false,        // 是否显示编辑器\n      imageUrl: '',             // 裁剪器的图片URL\n      selectedPreset: null,     // 选中的预设头像ID\n      avatarStyles: [           // 头像风格列表\n        {\n          id: 'micah',\n          name: '简约',\n          options: {\n            baseColor: ['f9c9b6', 'ac6651'],\n            mouth: ['laughing', 'smile', 'smirk'],\n            hair: ['thick', 'short'],\n            hairColor: ['0e0e0e', '77311d', 'fc909f']\n          }\n        },\n        {\n          id: 'personas',\n          name: '人物',\n          options: {\n            backgroundColor: ['b6e3f4', 'c0aede', 'ffd5dc'],\n            clothing: ['dress', 'dressShirt', 'graphic', 'hoodie', 'shirt', 'tankTop'],\n            clothingColor: ['545454', '65c9ff', 'ffd35c', 'ff6b6b'],\n            face: ['smile', 'laugh', 'cute']\n          }\n        },\n        {\n          id: 'identicon',\n          name: '几何',\n          options: {\n            colors: ['ff5733', '4ecdc4', '45b7d1', 'ffd93d'],\n            size: 6\n          }\n        },\n        {\n          id: 'thumbs',\n          name: '简笔',\n          options: {\n            backgroundColor: ['ffd5dc', 'b6e3f4', 'c0aede'],\n            shapeColor: ['ff5733', '4ecdc4', '45b7d1'],\n            mood: ['happy', 'surprised', 'chill']\n          }\n        }\n      ]\n    }\n  },\n  methods: {\n    // 触发文件选择\n    triggerFileInput() {\n      this.$refs.fileInput.click()\n    },\n\n    // 处理文件选择\n    handleFileSelect(event) {\n      const file = event.target.files[0]\n      if (file && file.type.startsWith('image/')) {\n        this.selectedFile = file\n        this.selectedPreset = null\n        this.showEditor = true\n        this.imageUrl = URL.createObjectURL(file)\n      }\n    },\n\n    // 处理裁剪变化\n    onChange({ canvas }) {\n      if (canvas) {\n        this.previewUrl = canvas.toDataURL()\n      }\n    },\n\n    // 保存头像\n    async saveAvatar() {\n      if (this.previewUrl) {\n        this.showEditor = false\n        this.$emit('avatar-selected', this.previewUrl)\n      }\n    },\n\n    // 取消编辑\n    cancelEdit() {\n      this.showEditor = false\n      this.selectedFile = null\n      this.imageUrl = ''\n      if (!this.selectedPreset) {\n        this.previewUrl = null\n      }\n      if (this.$refs.fileInput) {\n        this.$refs.fileInput.value = ''\n      }\n    },\n\n    // 选择预设头像\n    selectPresetAvatar(style) {\n      this.selectedPreset = style.id\n      this.selectedFile = null\n      this.showEditor = false\n      const avatarUrl = this.createAvatarUrl(style)\n      this.previewUrl = avatarUrl\n      this.$emit('avatar-selected', avatarUrl)\n    },\n\n    // 创建头像URL\n    createAvatarUrl(style) {\n      try {\n        const seed = this.username || Math.random().toString(36).substring(7)\n        const options = {\n          ...style.options,\n          size: 128\n        }\n        \n        // 使用最新的 DiceBear API\n        return `https://api.dicebear.com/7.x/${style.id}/svg?seed=${seed}&${this.serializeOptions(options)}`\n      } catch (error) {\n        console.error(`创建头像URL时出错 (${style.id}):`, error)\n        return ''\n      }\n    },\n\n    // 序列化选项\n    serializeOptions(options) {\n      return Object.entries(options)\n        .map(([key, value]) => {\n          if (Array.isArray(value)) {\n            return `${key}=${value.join(',')}`\n          }\n          return `${key}=${value}`\n        })\n        .join('&')\n    },\n\n    // 处理头像加载错误\n    handleAvatarError(styleId) {\n      console.error(`头像风格 ${styleId} 加载失败`)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.avatar-selector {\n  width: 100%;\n  max-width: 500px;\n  padding: 1.5rem;\n  background-color: var(--input-background);\n  border-radius: var(--card-border-radius);\n  box-shadow: var(--message-shadow);\n}\n\n.avatar-preview {\n  width: 150px;\n  height: 150px;\n  margin: 0 auto 2rem;\n  border-radius: 25px;\n  overflow: hidden;\n  border: 3px dashed var(--primary-color);\n  background: rgba(255, 255, 255, 0.05);\n  backdrop-filter: blur(5px);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  cursor: pointer;\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n  position: relative;\n}\n\n.avatar-preview:hover {\n  border-color: var(--secondary-color);\n  transform: scale(1.02);\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.15);\n}\n\n.preview-image {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  transition: all 0.3s ease;\n}\n\n.preview-image:hover {\n  transform: scale(1.05);\n}\n\n.empty-preview {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  height: 100%;\n  padding: 20px;\n  background: linear-gradient(\n    45deg,\n    rgba(var(--primary-color-rgb), 0.1),\n    rgba(var(--secondary-color-rgb), 0.1)\n  );\n  transition: all 0.3s ease;\n}\n\n.upload-icon {\n  width: 50px;\n  height: 50px;\n  margin-bottom: 12px;\n  position: relative;\n  transition: all 0.3s ease;\n}\n\n.plus-horizontal,\n.plus-vertical {\n  position: absolute;\n  background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));\n  border-radius: 3px;\n  transition: all 0.3s ease;\n}\n\n.plus-horizontal {\n  width: 24px;\n  height: 2px;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.plus-vertical {\n  width: 2px;\n  height: 24px;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.empty-preview:hover {\n  background-color: var(--other-message-bg);\n}\n\n.empty-preview:hover .plus-horizontal,\n.empty-preview:hover .plus-vertical {\n  background: linear-gradient(90deg, var(--secondary-color), var(--primary-color));\n  transform: scale(1.1);\n}\n\n.empty-preview span {\n  font-size: 1rem;\n  color: var(--text-color);\n  opacity: 0.9;\n  font-weight: 500;\n  transition: all 0.3s ease;\n  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);\n}\n\n.empty-preview:hover span {\n  opacity: 1;\n  transform: translateY(2px);\n  color: var(--primary-color);\n}\n\n.avatar-upload {\n  margin-bottom: 1.5rem;\n}\n\n.file-input {\n  display: none;\n}\n\n.editor-modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.8);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.editor-container {\n  background-color: var(--chat-background);\n  padding: 2rem;\n  border-radius: var(--card-border-radius);\n  box-shadow: var(--message-shadow);\n  max-width: 90vw;\n  max-height: 90vh;\n  overflow: auto;\n}\n\n.editor-controls {\n  margin-top: 1.5rem;\n}\n\n.editor-buttons {\n  display: flex;\n  gap: 1rem;\n}\n\n.save-button,\n.cancel-button {\n  padding: 0.5rem 1rem;\n  border: none;\n  border-radius: var(--card-border-radius);\n  cursor: pointer;\n  transition: all 0.2s ease;\n}\n\n.save-button {\n  background-color: var(--primary-color);\n  color: white;\n}\n\n.save-button:hover {\n  background-color: var(--secondary-color);\n}\n\n.cancel-button {\n  background-color: var(--other-message-bg);\n  color: var(--text-color);\n}\n\n.cancel-button:hover {\n  background-color: var(--input-background);\n}\n\n.preset-avatars {\n  margin-top: 2rem;\n  background-color: rgba(0, 0, 0, 0.1);\n  border-radius: var(--card-border-radius);\n  padding: 1.5rem;\n  backdrop-filter: blur(10px);\n  border: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n.preset-avatars h3 {\n  color: var(--text-color);\n  margin-bottom: 2rem;\n  font-size: 1.2rem;\n  text-align: center;\n  position: relative;\n  font-weight: 500;\n  letter-spacing: 0.5px;\n}\n\n.preset-avatars h3::after {\n  content: '';\n  position: absolute;\n  bottom: -0.75rem;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 50px;\n  height: 3px;\n  background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));\n  border-radius: 2px;\n}\n\n.avatar-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));\n  gap: 2rem;\n  padding: 1rem;\n}\n\n.preset-avatar-wrapper {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 0.5rem;\n}\n\n.avatar-preview-container {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  border-radius: 50%;\n  overflow